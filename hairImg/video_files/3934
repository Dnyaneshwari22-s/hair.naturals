        snapOptions.secondErrorWidgetPage = `undefined`;
        snapOptions.secondStyle = `undefined`;
        snapOptions.secondPosition = 'undefined';
        snapOptions.secondMobileDivId = 'undefined';
          snapOptions.showPaywidgetEmi= 'true';
