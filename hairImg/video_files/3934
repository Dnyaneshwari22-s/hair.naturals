    let month = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
    if(fullMonth){
        month = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
    }
    var dt = new Date(d.getTime());
    dt.setDate(28);
    dt.setMonth(dt.getMonth() + n);
    return month[dt.getMonth()];
}

function validatePhoneNumbers(input_str) {
    var re = /^[0-9]{10}$/;
    return re.test(input_str);
}

function getByIdOrClassName(input_str) {
    try {
        return document.querySelector(input_str);
    } catch (e) {
        console.log(e);
        return null;
    }
}
function parseNum(str) {
    if(snapOptions.genericPriceParseFunction && snapOptions.genericPriceParseFunction !== 'undefined'){
        return eval(snapOptions.genericPriceParseFunction);
    }
    str = str.toString();
    if (str.toLowerCase().indexOf('rs') > -1) {
        str = str.replace(/[^0-9.]/g, '');
        return parseInt(str.substring(str.indexOf('.') + 1));
    }
    str = str.toString().replace(/[^\d.-]/g, '');
    return parseInt(str);
}

function getNestedCategoryPrice (priceText, regex) {
    const priceMatch = priceText.match(regex);
    return priceMatch ? parseInt(priceMatch[1].replace(/,/g, ''), 10) : 'undefined';
}

function findPopup() {
    let popup = 0;
    for (let planIndex = 1; planIndex <= snapOptions.plans.length; planIndex++) {
        const plan = snapOptions.plans[planIndex - 1];
        if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
            popup++;
        }
    }
    if (snapOptions.pdpPayLaterAmount && snapOptions.pdpPayLaterAmount !== 'undefined') {
        if (popup === 1 && event && event.target && event.target.classList.contains('pay-later-emi-details-btn')) {
            popup = 2;
            if(snapOptions.addingDynamicPlan && snapOptions.addingDynamicPlan !== 'undefined'){
               popup = 3;
            }
        }
    }
    if (snapOptions.urlToShowWidgetExpress && snapOptions.urlToShowWidgetExpress !== 'undefined') {
        if (!window.location.href.includes(snapOptions.cartUrl)) {
            if (changeExpressWidget(snapOptions.urlToShowWidgetExpress)) {
                popup = 2;
            }
        }
    }
    if (snapOptions.cartWidgetExpressUrl && snapOptions.cartWidgetExpressUrl !== 'undefined') {
        if (window.location.href.includes(snapOptions.cartUrl)) {
            if (checkWhichWidgetToLoad(snapOptions.cartWidgetExpressUrl)) {
                popup = 2;
            }
        }
    }
    if (snapOptions.miniCartWidgetExpressUrl && snapOptions.miniCartWidgetExpressUrl !== 'undefined') {
        if (checkWhichWidgetToLoadMiniCart(snapOptions.miniCartWidgetExpressUrl)) {
            popup = 2;
        }
    }

        return popup;
}

const getPopupHtml = (element)=>{
    if(snapOptions.fetchPopupDetails && snapOptions.fetchPopupDetails !== 'undefined'){
        return eval(snapOptions.fetchPopupDetails);
    }
    if (snapOptions.isVarientPopup && snapOptions.isVarientPopup === 'true') {
    if(document.querySelector(snapOptions.variantParentDivId).innerHTML.trim() === ''){
          return snapOptions.normalPopup;
    }}
    let elementmini = getByIdOrClassName('#sm-widget-btn.snap_emi_txt_minicart ');
    let elementSecond = getByIdOrClassName('.second_widget_btn');
    if (element && element.classList.contains('snap_emi_txt_minicart')) {
        let elementmini = getByIdOrClassName('#sm-widget-btn.snap_emi_txt_minicart ');
        if (elementmini) {
            var orderVal = elementmini.getAttribute("data-widget_mini");
        }

    } else if (element && !element.classList.contains('snap_emi_txt_minicart')) {        
              if (element && element.querySelector('.dp-collection-price')) {
            let getCurrentPrice = element.querySelector('.dp-collection-price').getAttribute('data-price');
            if (getCurrentPrice) {
               var orderVal = parseNum(getCurrentPrice);
              
            }
        }else{
        let element = getByIdOrClassName('#sm-widget-btn.snap_emi_txt ');
        var orderVal = element.getAttribute("data-widget");
        if (!orderVal) {
            let element = getByIdOrClassName('#sm-widget-btn.second_widget_btn ');
            var orderVal = element.getAttribute("data-widget");
        }
    }
    } else {
        let element = getByIdOrClassName('#sm-widget-btn.snap_emi_txt ');
        var orderVal = element.getAttribute("data-widget");
        if(!orderVal)
        {
let element = getByIdOrClassName('#sm-widget-btn.second_widget_btn ');
        var orderVal = element.getAttribute("data-widget");
        }
    }
    // let popup = findPopup(orderVal);
    let popup = 0;
    let showPayLaterPopUp = false;
    for (let planIndex = 1; planIndex <= snapOptions.plans.length; planIndex++) {

        const plan = snapOptions.plans[planIndex - 1];
        if ((parseInt(orderVal)) >= plan.minAmount && (parseInt(orderVal)) <= plan.maxAmount) {
            popup++;
        }
    }

    if (snapOptions.pdpPayLaterAmount && snapOptions.pdpPayLaterAmount !== 'undefined') {
        if (parseInt(orderVal) < parseInt(snapOptions.pdpPayLaterAmount)) {
            showPayLaterPopUp = true;
        } else {
            showPayLaterPopUp = false;
            if (popup === 1 && event && event.target && event.target.classList.contains('pay-later-emi-details-btn')) {
                popup = 2;
                if (snapOptions.addingDynamicPlan && snapOptions.addingDynamicPlan !== 'undefined') {
                    popup = 3;
                }
            }
        }
    }
    if (snapOptions.urlToShowWidgetExpress && snapOptions.urlToShowWidgetExpress !== 'undefined') {
        if (!window.location.href.includes(snapOptions.cartUrl)) {
            if (changeExpressWidget(snapOptions.urlToShowWidgetExpress)) {
                popup = 2;
            }
        }
    }
    if (snapOptions.cartWidgetExpressUrl && snapOptions.cartWidgetExpressUrl !== 'undefined') {
        if (window.location.href.includes(snapOptions.cartUrl)) {
            if (checkWhichWidgetToLoad(snapOptions.cartWidgetExpressUrl)) {
                popup = 2;
            }
        }
    }
    if (snapOptions.miniCartWidgetExpressUrl && snapOptions.miniCartWidgetExpressUrl !== 'undefined') {
        if (checkWhichWidgetToLoadMiniCart(snapOptions.miniCartWidgetExpressUrl)) {
            popup = 2;
        }
    }
    const isMiniCart = getByIdOrClassName(snapOptions.miniCartDivId);

    /** This below if condition checks if showPayLater is true then show payLaterPopUp */
    if (showPayLaterPopUp && snapOptions.payLaterPopUp && snapOptions.payLaterPopUp !== 'undefined') {
        return snapOptions.payLaterPopUp;
    } else {
        showPayLaterPopUp = false;
    }
    if (!isMiniCart) {
        let callFunction = true;
        if (snapOptions.categoryWidgetHtml && snapOptions.categoryWidgetHtml !== 'undefined') {
            if (element && element.querySelector('.dp-collection-price')) {
                callFunction = false;
                switch(popup) {
                    case 1:
                        return snapOptions.categoryPopup;
                    case 2:
                        return snapOptions.categoryPopup2;
                    case 3: 
                        return snapOptions.categoryPopup3;
                    case 4: 
                        return snapOptions.categoryPopup4;
                    default:
                        return snapOptions.categoryPopup; 
                }
            }
        }
        if (callFunction) {
            loadOnPage();
        }
    }
    if (popup === 0) {
        return;
    }
    if (isMiniCart) {
        switch (popup) {
        case 1:
            return snapOptions.miniCartPopupPage1;
        case 2:
            return snapOptions.miniCartPopupPage2;
        case 3:
            return snapOptions.miniCartPopupPage3;
        case 4:
            return snapOptions.miniCartPopupPage4;
        case 5:
             return snapOptions.secondPopupPage5;
        default:
            return snapOptions.miniCartPopupPage1;
        }
    } else {
        if (snapOptions.isTwoWidget === 'true' && secondWidgetClick(element)) {
            switch (popup) {
            case 1:
                return snapOptions.secondPopupPage1;
            case 2:
                return snapOptions.secondPopupPage2;
            case 3:
                return snapOptions.secondPopupPage3;
            case 4:
                return snapOptions.secondPopupPage4;
             case 5:
                return snapOptions.secondPopupPage5;
            default:
                return snapOptions.secondPopupPage1;
            }
        } else {
            switch (popup) {
            case 1:
                return snapOptions.popupPage1;
            case 2:
                return snapOptions.popupPage2;
            case 3:
                return snapOptions.popupPage3;
            case 4:
             return snapOptions.popupPage4;
             case 5:
             return snapOptions.secondPopupPage5;
            default:
                return snapOptions.popupPage1;
            }
        }
    }
}

const getPriceArray = () => {
    const widgetPriceBreakdown = snapOptions.widgetPriceBreakdown;
    return widgetPriceBreakdown.split(',').map(Number);
};

const removeExistingElements = (selector) => {
    let element = getByIdOrClassName(selector);
    if (element) element.parentNode.removeChild(element);
    
    if (snapOptions.isTwoWidget === 'true') {
        let newElement = getByIdOrClassName('.second_widget_btn');
        if (newElement) newElement.parentNode.removeChild(newElement);
    }
};

const getWidgetForPriceRange = (snapOrderValue, priceArray, widgetPrefix, fallbackWidget) => {

    if (priceArray.length === 1) {
        if (snapOrderValue <= priceArray[0]) {
            return snapOptions[`${widgetPrefix}1`] || fallbackWidget;
        } else {
            return snapOptions[`${widgetPrefix}2`] || fallbackWidget;
        }
    }

    if (snapOrderValue <= priceArray[0]) {
        return snapOptions[`${widgetPrefix}1`] || fallbackWidget;
    }

    for (let i = 0; i < priceArray.length - 1; i++) {
        if (snapOrderValue > priceArray[i] && snapOrderValue <= priceArray[i + 1]) {
            const widgetIndex = i + 2;
            return snapOptions[`${widgetPrefix}${widgetIndex}`] || snapOptions[`${widgetPrefix}2`] || fallbackWidget;
        }
    }

    if (snapOrderValue >= priceArray[priceArray.length - 1]) {
        return snapOptions[`${widgetPrefix}3`] || fallbackWidget;
    }

    return fallbackWidget;
};


const snapGetWidgetHtml = (snapOrderValue) => {
    const priceArray = getPriceArray();
    const isMiniCart = getByIdOrClassName(snapOptions.miniCartDivId);
    const isCategoryPage = getByIdOrClassName(snapOptions.categoryWidgetInfo);
    const isCartPage = window.location.href.includes(snapOptions.cartUrl);
    
    removeExistingElements("#sm-widget-btn");

    if (!isCartPage && !isMiniCart) {
        return getWidgetForPriceRange(snapOrderValue, priceArray, 'pdpWidget', snapOptions.pdpWidget2);
    }
    if (isCartPage) {
        return getWidgetForPriceRange(snapOrderValue, priceArray, 'cartWidget', snapOptions.pdpWidget2); 
    }
    if (isCategoryPage) {
        return getWidgetForPriceRange(snapOrderValue, priceArray, 'categoryWidget', snapOptions.categoryWidget2);
    }
};

function checkWhichWidgetToLoad(cartWidgetExpressUrl) {
    let value = false;
    let allElement = document.querySelectorAll(cartWidgetExpressUrl);
    if (allElement.length > 0) {
        allElement.forEach((element) => {
            let list = snapOptions.cartWidgetShowText.split(',');
            list.forEach((item) => {
                if (element.href) {
                    if (element.href.includes(item)) {
                        value = true;
                    }
                }
            });
        });
    }
    return value;
}
function getQuantity(isMiniCart, secondQuantityDivId='') {
    let quantityValue = getByIdOrClassName(
        isMiniCart ? snapOptions.miniCartQuantityDivId : snapOptions.quantityDivId
    );
    if(secondQuantityDivId){
        quantityValue = getByIdOrClassName(secondQuantityDivId);
    }
    if (quantityValue.nodeName === 'INPUT') {
        snapQuantity = quantityValue.value;
    } else {
        snapQuantity = quantityValue.innerText;
    }
    try {
         snapQuantity = parseInt(snapQuantity);
        if(snapQuantity==0)
        {
            snapQuantity=1;
        }
    } catch (e) {
        snapQuantity = 1;
    }
}
window.addEventListener('message', (event) => {
    if (event.data.closeIframe) {
        if (snapOptions.isFunAlready && snapOptions.isFunAlready !== 'undefined') {
            snapclosePopup();
        } else {
            closePopup();
        }
    }
});
window.addEventListener('message', (event) => {
    if (
        event.origin.startsWith('http://localhost:3000') ||
        event.origin.startsWith('https://qacheckout.snapmint.com') ||
        event.origin.startsWith('https://checkout.snapmint.com') ||
        event.origin.startsWith("https://pay.snapmint.com")
    ) {
        if (event.data.closeIframe) {
            closeCheckV2Iframe();
        } else if (event.data.dpPageVisible) {
            if (event.data.dpPageVisible == 'open') {
                document.getElementsByClassName('popuppiza')[0].style.width = '100%';
                document.getElementsByClassName('popuppiza')[0].style.height = '500px';
            } else {
                document.getElementsByClassName('popuppiza')[0].style.width = 'fit-content';
                document.getElementsByClassName('popuppiza')[0].style.height = '100vh';
            }
        } else if (event.data.postRedirect) {
            closeCheckV2Iframe();
            if (window && event.data.url) {
                if (event.data.params && event.data.params.length > 0) {
                    var form = document.createElement('form');
                    form.setAttribute('method', 'post');
                    form.setAttribute('action', event.data.url);
                    const params = event.data.params;
                    for (var i in params) {
                        if (params.hasOwnProperty(i)) {
                            var input = document.createElement('input');
                            input.type = 'hidden';
                            input.name = i;
                            input.value = params[i];
                            form.appendChild(input);
                        }
                    }
                    document.body.appendChild(form);
                    form.submit();
                    document.body.removeChild(form);
                } else {
                    window.location.assign(event.data.url);
                }
            }
        }
    } else {
        return;
    }
});

function setUtmReferCookie() {
    try {
        if (
            window.location.href.includes('gclid') ||
            window.location.href.includes('utm_source') ||
            window.location.href.includes('fbclid')
        ) {
            const snap_landing_page = window.location.href;
            let snap_referrer_url = '';
            try {
                snap_referrer_url = document.cookie.substring(document.cookie.indexOf('refer')).split(';')[0];
            } catch (err) {
                console.log(err);
            }

            var now = new Date();
            var minutes = 60 * 24 * 30;
            now.setTime(now.getTime() + minutes * 60 * 1000);

            expires = '; expires=' + now.toUTCString();
            document.cookie = 'snap_landing_page' + '=' + (snap_landing_page || '') + expires + '; path=/';
            document.cookie = 'snap_referrer_url' + '=' + (snap_referrer_url || '') + expires + '; path=/';
        }
    } catch (err) {
        console.log(err);
    }
}
function navigatewindow() {
    if (windowPopup && !windowPopup.closed) {
        windowPopup.focus();
        window.localStorage.removeItem('autosnapminturl');
    } else {
        var x = window.localStorage.getItem('snapminturl');
        var autourl = window.localStorage.getItem('autosnapminturl');
        if (autourl) {
            windowPopup = window.open(autourl, 'mozillaWindow', windowFeaturesSnapmint);
            checkpopup(windowPopup, autourl);
            window.localStorage.removeItem('autosnapminturl');
            if (document.getElementById('snap-modalon_page'))
                document.getElementById('snap-modalon_page').style.display = 'none';
            document.body.style.overflow = 'auto';
            document.getElementsByTagName('html')[0].style.overflow = 'auto';
        } else if (x) {
            var decrypted = CryptoJS.AES.decrypt(x, encryptionKeyCryptoJs);
            windowPopup = window.open(decrypted.toString(CryptoJS.enc.Utf8), 'mozillaWindow', windowFeaturesSnapmint);
            checkpopup(windowPopup, decrypted.toString(CryptoJS.enc.Utf8));
            document.getElementsByTagName('html')[0].style.overflow = 'auto';
        } else {
            window.localStorage.removeItem('autosnapminturl');
            if (document.getElementById('snap-modalon_page'))
                document.getElementById('snap-modalon_page').style.display = 'none';
            document.body.style.overflow = 'auto';
            document.getElementsByTagName('html')[0].style.overflow = 'auto';
            return false;
        }
    }
}
function getDownPaymentAndEmi(orderValue = 0) {
    for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
        const plan = snapOptions.plans[planIndex];
        if (orderValue >= plan.minAmount && orderValue <= plan.maxAmount) {
            downPayment = Math.round((orderValue * plan.dpPercent) / 100);
            emi = Math.round(parseFloat(orderValue * plan.emiPercent) / 100);
        }
    }
    if (!downPayment) {
        downPayment = orderValue / 10;
    }
    if (!emi) {
        emi = parseFloat(orderValue * 10) / 100.0;
    }
}function loadGtag() {
    var newScript = document.createElement("script");
    newScript.type = "text/javascript";
    newScript.setAttribute("async", "true");
    newScript.setAttribute("src", "https://www.googletagmanager.com/gtag/js?id=GTM-MBDRX7M");
    document.getElementsByTagName('head')[0].appendChild(newScript);
}

function gtmInitiate(){
    var s = document.createElement('script');
    s.type = 'text/javascript';
    var code = "window.dataLayer = window.dataLayer || [];function snapGtag(){dataLayer.push(arguments);}snapGtag('js', new Date());snapGtag('config', 'GTM-MBDRX7M');"
    try {
        s.appendChild(document.createTextNode(code));
        document.body.appendChild(s);
    } catch (e) {
        s.text = code;
        document.body.appendChild(s);
    }
}
function webEngageInitiate(){
    if(typeof webengage === 'undefined'){
    !function(w,e,b,n,g){function o(e,t){e[t[t.length-1]]=function(){r.__queue.push([t.join("."),
    arguments])}}var i,s,r=w[b],z=" ",l="init options track screen onReady".split(z),a="feedback survey notification".split(z),c="options render clear abort".split(z),p="Open Close Submit Complete View Click".split(z),u="identify login logout setAttribute".split(z);if(!r||!r.__v){for(w[b]=r={__queue:[],__v:"6.0",user:{}},i=0;i < l.length;i++)o(r,[l[i]]);for(i=0;i < a.length;i++){for(r[a[i]]={},s=0;s < c.length;s++)o(r[a[i]],[a[i],c[s]]);for(s=0;s < p.length;s++)o(r[a[i]],[a[i],"on"+p[s]])}for(i=0;i < u.length;i++)o(r.user,["user",u[i]]);setTimeout(function(){var f=e.createElement("script"),d=e.getElementById("_webengage_script_tag");f.type="text/javascript",f.async=!0,f.src=("https:"==e.location.protocol?"https://widgets.in.webengage.com":"http://widgets.in.webengage.com")+"/js/webengage-min-v-6.0.js",d.parentNode.insertBefore(f,d)})}}(window,document,"webengage");
    }
    if(snapOptions.isWebEngageEnable && snapOptions.isWebEngageEnable  !=='undefined'){
        if(typeof webengage !== 'undefined'){
        webengage.init(snapOptions.isWebEngageEnable);
        }
    }
}


function captureEvents(eventName, eventProperties = {}) {
         if (!window.posthog){
        addPosthogEvent() ;
       }
    let snapUnleashId = window.localStorage.getItem('unleash:repository:sessionId');
    posthog.capture(eventName, { ...eventProperties, variant: snapUnleashFlag, snapUnleashSessionId: snapUnleashId });
}

function addPosthogEvent() {
    //check if posthog is already loaded or not
    if (
        !(
            window.posthog &&
            window.posthog.config &&
            window.posthog.config.api_host &&
            window.posthog.config.api_host.indexOf('snapmint') > -1
        )
    ) {
        !(function (t, e) {
            var o, n, p, r;
            e.__SV ||
                ((window.posthog = e),
                (e._i = []),
                (e.init = function (i, s, a) {
                    function g(t, e) {
                        var o = e.split('.');
                        2 == o.length && ((t = t[o[0]]), (e = o[1])),
                            (t[e] = function () {
                                t.push([e].concat(Array.prototype.slice.call(arguments, 0)));
                            });
                    }
                    ((p = t.createElement('script')).type = 'text/javascript'),
                        (p.async = !0),
                        (p.src = s.api_host + '/static/array.js'),
                        (r = t.getElementsByTagName('script')[0]).parentNode.insertBefore(p, r);
                    var u = e;
                    for (
                        void 0 !== a ? (u = e[a] = []) : (a = 'posthog'),
                            u.people = u.people || [],
                            u.toString = function (t) {
                                var e = 'posthog';
                                return 'posthog' !== a && (e += '.' + a), t || (e += ' (stub)'), e;
                            },
                            u.people.toString = function () {
                                return u.toString(1) + '.people (stub)';
                            },
                            o =
                                'capture identify alias people.set people.set_once set_config register register_once unregister opt_out_capturing has_opted_out_capturing opt_in_capturing reset isFeatureEnabled onFeatureFlags'.split(
                                    ' '
                                ),
                            n = 0;
                        n < o.length;
                        n++
                    )
                        g(u, o[n]);
                    e._i.push([i, s, a]);
                }),
                (e.__SV = 1));
        })(document, window.posthog || []);
        //postHog prod key
        let postHogKey = 'phc_OLwDl0hfuS6vJlFnYx2QnxxlJbQJPgTk0lbkBzCG8LC';
        //postHog prod api host
        let postHogApiHost = 'https://snapmint-delta-posthog.ap-south-1.aws.opsverse.cloud';
        if (snapOptions.postHogKey && snapOptions.postHogKey !== 'undefined') postHogKey = snapOptions.postHogKey;
        if (snapOptions.postHogUrl && snapOptions.postHogUrl !== 'undefined') postHogApiHost = snapOptions.postHogUrl;
        posthog.init(postHogKey, {
            api_host: postHogApiHost,
            autocapture: false,
            capture_pageview: false,
            capture_pageleave: false
        });
        if (
            snapOptions.isPosthogEventRequired &&
            snapOptions.isPosthogEventRequired === 'true' &&
            (snapOptions.unleashAbTest === 'undefined' || snapOptions.unleashAbTest === false)
        ) {
            if (!window.location.href.includes(snapOptions.cartUrl)) {
                snapWidgetSeenEvent();
            }
        }
    }
}

function snapWidgetSeenEvent() {
    if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
        //capture event for pdp widget seen on page
        captureEvents('pdp_widget_seen', {
            merchant_id: snapOptions.mid,
            merchant_name: snapOptions.merchantName
        });
    }
}

async function loadUnleashScript() {
    return new Promise((resolve, reject) => {
        let script = document.createElement('script');
        script.src = 'https://unpkg.com/unleash-proxy-client@latest/build/main.min.js';
        script.onload = resolve;
        script.onerror = reject;
        document.body.appendChild(script);
    });
}

async function snapAddUnleashScript() {
    if (
        snapOptions.unleashAbTest &&
        snapOptions.unleashAbTest !== 'undefined' &&
        !window.location.href.includes(snapOptions.cartUrl) &&
        snapUnleashFlag !== ''
    ) {
        snapAbTestTextChange();
        return;
    }
    // Load Unleash script
    try {
        await loadUnleashScript();
    } catch (error) {
        console.error('Failed to load Unleash script:', error);
        snapAbTestTextChange();
        return;
    }

    var config = {
        url: 'https://unleash-proxy-prod.snapmint.com/proxy',
        clientKey: 'proxy-client-key',
        appName: 'my-webapp'
    };
    let id = setTimeout(() => {
        var snapmint_Unleash_client = new unleash.UnleashClient(config);
        snapmint_Unleash_client.on('update', () => {
            snapUnleashFlag = snapmint_Unleash_client?.getVariant('widget_ab_test')?.name;
            if (
                snapOptions.unleashAbTest &&
                snapOptions.unleashAbTest !== 'undefined' &&
                !window.location.href.includes(snapOptions.cartUrl)
            ) {
                snapAbTestTextChange();
            }
        });
        // loadOnPage();
        snapmint_Unleash_client.start();
        clearTimeout(id);
    }, 500);
    if (snapUnleashFlag && snapUnleashFlag === '') {
        clearTimeout(id);
    }
}

function snapAbTestTextChange() {
    let widgetPage = '';
    var snapWidget = document.querySelector('#sm-widget-btn');
    const minOrderVal = parseNum(snapOptions.plans[0].minAmount);
    let parentDiv = getByIdOrClassName(snapOptions.divId);
    if (checkPdpIsMobile()) parentDiv = getByIdOrClassName(snapOptions.mobileDivId);
    if (minOrderVal > snapOrderValue) {
        let errorWidget = snapOptions.errorWidgetPage;
        getByID('dp').textContent = downPayment;
        parentDiv.innerHTML = errorWidget;
        return;
    }
    console.log({ snapUnleashFlag });
    if (snapUnleashFlag && snapOptions[snapUnleashFlag] && snapOptions[snapUnleashFlag] !== 'undefined') {
        widgetPage = snapOptions[snapUnleashFlag];
    } else {
        widgetPage = snapOptions.widgetPage;
        if (
            !window.location.href.includes(snapOptions.cartUrl) &&
            snapOptions.pdpPayLaterAmount &&
            snapOptions.pdpPayLaterAmount !== 'undefined'
        ) {
            widgetPage = getPayLaterWidget(snapOptions.pdpPayLaterAmount, snapOptions.widgetPage, 'pdp');
        } else if (
            window.location.href.includes(snapOptions.cartUrl) &&
            snapOptions.cartPayLaterAmount &&
            snapOptions.cartPayLaterAmount !== 'undefined'
        ) {
            widgetPage = getPayLaterWidget(snapOptions.cartPayLaterAmount, snapOptions.widgetPage, 'cart');
        }
        if (snapOptions.urlToShowWidgetExpress && snapOptions.urlToShowWidgetExpress !== 'undefined') {
            if (!window.location.href.includes(snapOptions.cartUrl)) {
                if (changeExpressWidget(snapOptions.urlToShowWidgetExpress)) {
                    widgetPage = snapOptions.widgetExpressUrl;
                    showWidgetExpress = true;
                }
            }
        }
        if (snapOptions.cartWidgetExpressUrl && snapOptions.cartWidgetExpressUrl !== 'undefined') {
            if (window.location.href.includes(snapOptions.cartUrl)) {
                if (checkWhichWidgetToLoad(snapOptions.cartWidgetExpressUrl)) {
                    widgetPage = snapOptions.checkForCartWidget;
                    showWidgetExpress = true;
                } else {
                    showWidgetExpress = false;
                }
            }
        }
    }
    if (!getByIdOrClassName('#sm-widget-btn') && widgetPage) {
        if (snapOptions.position === 'before') parentDiv.insertAdjacentHTML('beforebegin', widgetPage);
        else parentDiv.insertAdjacentHTML('afterend', widgetPage);
        addingCssStyle(snapOptions.style, '#sm-widget-btn');
        snapWidgetSeenEvent();
        if (snapWidget) {
            snapWidget.style.visibility = 'visible';
        }
    }
    // after Abtest widget is added to DOM, for checking isTwoWidget, pdpCashbackText, cartCashbackText, merchantLogo, addingDynamicPlan, isAbTest, cartWidgetExpressUrl, isCashbackCoupon & pdpPayLaterAmount
    loadDataAfterWidgetLoad();
}

function getFbcParameter() {
    var urlParams = new URLSearchParams(window.location.search);
    return urlParams.get('fbc');
}
function storeFbcParameter() {
    var fbc = getFbcParameter();
    if (fbc) {
        document.cookie = "snapmintfbc=" + fbc + "; path=/; expires=Fri, 31 Dec 9999 23:59:59 GMT";
    }
}
async function webEngageEvent(eventName, data) {
    if (snapOptions.isWebEngageEnable && snapOptions.isWebEngageEnable !== 'undefined') {
        const webEngageProps ={
            ...data,
            product_variant_id: getFilterd(snapProducts, 'imei1'),
            product_name: getFilterd(snapProducts, 'name'),
            product_quantity: getFilterd(snapProducts, 'quantity'),
            product_price: getFilterd(snapProducts, 'unit_price'),
            product_url : getFilterd(webEngageInfo, 'product_url'),
            product_image : getFilterd(webEngageInfo, 'image_url'),
            product_id : getFilterd(webEngageInfo, 'product_id'),
        }
        webengage.track(eventName, webEngageProps);
    }
    if (snapOptions.isMetaPixelEnable && snapOptions.isMetaPixelEnable !== 'undefined') {
        storeFbcParameter();
        if (typeof fbq === 'function') {
            let productData = {}
            if(snapProducts.length > 0 && snapProducts[0].source !== 'cart') {
                const currentUrl = window.location.href;
                const newUrl = currentUrl.split('?')[0] + '.json';
                const response = await fetch(newUrl);
                if (!response.ok) {
                    throw new Error('Network response was not ok ' + response.statusText);
                }
                productData = await response.json();
            }
            const props = {
                content_category: productData?.product?.product_type || null,
                content_ids: getFilterd(webEngageInfo, 'product_id'),
                content_name: getFilterd(snapProducts, 'name'),
                content_type: 'product_group',
                currency: 'INR',
                value: data.order_value
            }
            if(getDynamicJsCookie("snapmintfbc")){
                props['fbc'] = getDynamicJsCookie("snapmintfbc");
            }
            fbq('track', 'InitiateCheckout', props);
            if(snapProducts.length > 0 && snapProducts[0].source === 'cart'){
                fbq('track', 'AddToCart', props);
            }
        }
    }
}
function getFilterd(data, key) {
    let value = [];
    data.forEach((element) => {
        value.push(element[key]);
    });
    return value;
}
function totalOrderValue(snapOrderValue) {
    let order_value = document.querySelectorAll('.total_order_value');
    if (order_value && order_value.length > 0) {
        order_value.forEach((element) => {
            element.innerText = 'â‚¹' + snapOrderValue;
        });
    }

     let order_value_popup = document.querySelectorAll('.latest_2_plan_with_form .total_order_value_popup');
    if (order_value_popup && order_value_popup.length > 0) {
        order_value_popup.forEach((element) => {
            element.innerText = 'â‚¹' + snapOrderValue;
        });
    }
}
function changeValueAccordingToCoupon(snapOrderValue) {
    if (snapOrderValue) {
        let downPayment = 0;
        let emi = 0;
        for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];
            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                downPayment = Math.round((snapOrderValue * plan.dpPercent) / 100);
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
            }
        }

        if (!downPayment) {
            downPayment = snapOrderValue / 10;
        }

        if (!emi) {
            emi = parseFloat(snapOrderValue * 10) / 100.0;
        }
        replaceAllValuesInsideClass('downPayment', downPayment);
        replaceAllValuesInsideClass('emiAmount', emi);
        replaceValuesForNonExpress(downPayment, emi);
        addPlanType();
        if (getByIdOrClassName(snapOptions.miniCartDivId) || getByIdOrClassName('.snap_emi_txt_minicart')) {
            let element = document.querySelector('.snap_emi_txt_minicart');
            if (element) {
                if (element.classList.contains('coupon_applied_already')) {
                    element.classList.remove(element.classList[element.classList.length - 1]);
                    if (snapDiscountCode) {
                        element.classList.add(snapDiscountCode);
                        window.localStorage.setItem('currentDiscountCode', snapDiscountCode);
                    }
                } else {
                    element.classList.add('coupon_applied_already');
                    if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
                        if (snapDiscountCode) element.classList.add(snapDiscountCode);
                    } else {
                        element.classList.add(snapDiscountCode);
                    }
                    window.localStorage.setItem('currentDiscountCode', snapDiscountCode);
                }
            }
            let dp = document.querySelectorAll('.dp-class_minicart');
            if (dp.length > 0) {
                dp.forEach((element) => {
                    element.textContent = downPayment;
                });
            }
        } else {
            let element = document.querySelector('.snap_emi_txt');
            if (document.querySelector('.second_widget_btn')) element = document.querySelector('.second_widget_btn');
            if (element) {
                if (element.classList.contains('coupon_applied_already')) {
                    element.classList.remove(element.classList[element.classList.length - 1]);
                    if (snapDiscountCode) {
                        element.classList.add(snapDiscountCode);
                        window.localStorage.setItem('currentDiscountCode', snapDiscountCode);
                    }
                } else {
                    element.classList.add('coupon_applied_already');
                    element.classList.add(snapDiscountCode);
                    window.localStorage.setItem('currentDiscountCode', snapDiscountCode);
                }
            }
            let allDp = document.querySelectorAll('#dp');
            if (allDp) {
                allDp.forEach((widget) => {
                    widget.innerText = downPayment;
                });
            }
        }
    }
}
function addingDeliveryCharge() {
    if (snapOptions.deliveryAmount && snapOptions.deliveryAmount !== 'undefined') {
        let element = document.querySelectorAll('.snap_delivery_charges_text');
        if (snapOptions.minForDelivery && snapOptions.minForDelivery !== 'undefined') {
            if (parseInt(snapOrderValue) > parseInt(snapOptions.minForDelivery)) {
                return;
            }
        }
        if (element && element.length > 0) {
            element.forEach((element) => {
                element.innerText = `Delivery Fee of â‚¹${snapOptions.deliveryAmount} included for orders below â‚¹${snapOptions.minForDelivery}`;
            });
        }
    }
}
function addingSubtoalValue() {
    let element = document.querySelectorAll('.snap_subtotal_section_value');
    if (element) {
        for (let i = 0; i < element.length; i++) {
            element[i].innerText = 'â‚¹' + orderValueNotChange;
        }
    }
}
function checkCouponApplied(event, appliedCoupon, listLength) {
    try {
        if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
            if (onWebsiteCouponApplied) {
                return false;
            }
        }
        if (snapOptions.isCouponListNot && snapOptions.isCouponListNot !== 'undefined') {
            let applied_Coupon = document.querySelector('.coupon_applied_already');
            if (applied_Coupon && !appliedCoupon) {
                let coupoun = applied_Coupon.classList[applied_Coupon.classList.length - 1];
                if (coupoun && document.querySelector('.snap_coupon_input')) {
                    document.querySelector('.snap_coupon_input').value = coupoun.toUpperCase();
                    if (document.querySelector('.snap_coupon_apply_btn')) {
                        document.querySelector('.snap_coupon_apply_btn').click();
                    }
                }
            }
        } else if (listLength === 0) {
            let applied_Coupon = document.querySelector('.coupon_applied_already');
            if (applied_Coupon && !appliedCoupon) {
                let coupoun = applied_Coupon.classList[applied_Coupon.classList.length - 1];
                if (coupoun && document.querySelector('.snap_coupon_input')) {
                    document.querySelector('.snap_coupon_input').value = coupoun.toUpperCase();
                    if (document.querySelector('.snap_coupon_apply_btn')) {
                        document.querySelector('.snap_coupon_apply_btn').click();
                    }
                }
            }
        }
        if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
            var target = null;
            if (event && event.target) {
                target = event.target;
            } else {
                target = snapMintEvent;
            }
        } else {
            var target = event.target;
        }
        if (!target) {
            target = snapMintEvent;
        }
        let count = 0;
        while (target && !target.classList.contains('coupon_applied_already')) {
            count++;
            if (count > 5) break;
            target = target.parentElement;
        }
        if (target) {
            return target.classList.contains(appliedCoupon);
        }
        return false;
    } catch (err) {
        return false;
    }
}
function removeCoupon(snapDiscountCode) {
    if (document.querySelector('.snap_emi_txt_wrapper ')) {
        let allElements = document.querySelectorAll('.snap_emi_txt_wrapper');
        allElements.forEach((element) => {
            if (element.classList.contains('coupon_applied_already')) {
                element.classList.remove('coupon_applied_already');
            }
            if (element.classList.contains(snapDiscountCode)) {
                element.classList.remove(snapDiscountCode);
            }
        });
    }
}
function addingCashbackCoupon() {
    let amount = 0;
    if (snapOptions.deliveryAmount && snapOptions.deliveryAmount !== 'undefined') {
        if (parseInt(snapOrderValue) < parseInt(snapOptions.minForDelivery)) {
            amount = parseInt(snapOptions.deliveryAmount);
        }
    }
    totalOrderValue(parseInt(snapOrderValue) + parseInt(amount));
    addingDeliveryCharge();
    addingSubtoalValue();
    if (getDynamicJsCookie('snapmintDynamicJscookie')) {
        let element = document.querySelector('.snap_cupn_offers_btn');
        if (element) {
            document.querySelector('.snap_cupn_offers_btn').style.pointerEvents = 'none';
        }
    }
    if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
        if (onWebsiteCouponApplied && getByIdOrClassName(snapOptions.miniCartDivId)) {
            let element = document.querySelector('.snap_cupn_offers_btn');
            if (element) {
                document.querySelector('.snap_cupn_offers_btn').style.pointerEvents = 'none';
            }
        }
    }
    async function makeRequest(event) {
        try {
            var myHeaders = new Headers();
            myHeaders.append('Shopify-Shop-Domain', window.Shopify.shop);
            myHeaders.append('Content-Type', 'application/json');
            var requestOptions = {
                method: 'GET',
                headers: myHeaders,
                redirect: 'follow'
            };
            let response;
            if(snapOptions.couponDetails && snapOptions.couponDetails !== 'undefined'){
                eval(snapOptions.couponDetails);
            }else{
                response = await fetch('https://api.snapmint.com/v1/shopify_plugin/discount_codes', requestOptions);
            }
            if (!response.ok) {
                throw new Error(`Error! status: ${response.status}`);
            }
            if (response.status == 200) {
                //for particular coupon on particular product
                let result = await response.json();
                let coupens = [];
                if (snapOptions.particularCoupon && snapOptions.particularCoupon !== 'undefined') {
                    let coupoun = eval(snapOptions.particularCoupon);
                    let temp_result = [];
                    temp_result = result.data.filter((element) => element.discount_code == coupoun);
                    if (temp_result.length) {
                        result.data = temp_result;
                    } else {
                        result.data = [];
                    }
                }
                result.data.forEach((element) => {
                    var coups =
                        '<div class="snap_coupon_tkt ' +
                        element.discount_code +
                        '"> <div class="snap_tkt_left_section"> <img src="https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_tkt_left_part.svg" class="snap_tkt_left_section_img"> <div class="snap_coupon_info_prcent"> <div class="snap_wrpr_coupon"><img src="https://preemi.snapmint.com/assets/express_checkout/Discount_Images/Coupon_tkt_info.svg"><div class="snap_wrpr_txt_coupon">' +
                        element.off_text +
                        '</div></div></div></div><div class="snap_coupon_tkt_info"><div class="snap_coupon_code">' +
                        element.discount_code +
                        '</div> <div class="snap_display_flex" style="display:none"><img src="https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_code_applied_icon.svg"><div class="snap_coupon_code_applied_msg"><span class="snap_font_weight_bold snap_padding_right">â€˜SAVEBIGâ€™</span>Applied</div> </div><div class="snap_coupon_not_applicable_msg snap_applicable_error_msg_position" style="display:none">Coupon Not Applicable</div> <div class="snap_coupon_tkt_info_txt">' +
                        element.description +
                        '</div></div> <div class="snap_tkt_right_section"><img src="https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_tkt-right_part.svg"  class="snap_tkt_right_section_img" style="cursor:pointer;"><div class="snap_coupon_apply_btn snap_cursor_pointer Apply" id="' +
                        element.discount_code +
                        '">Apply</div></div></div>';
                    coupens.push(coups);
                    document.querySelector('.coupenDiv').innerHTML = coupens.join('');
                });

                if (snapOptions.newDiscountSummary && snapOptions.newDiscountSummary !== 'undefined') {
                    coupens = [];
                    snapMintCouponList = result.data;
                    result.data.forEach((element) => {
                        var coups = `<div class="snap_coupon_tkt ${element.discount_code}">
                        <div class="snap_coupon_tkt_info">
                            <div class="snap_coupon_code_wrpr">
                                <img src="https://preemi.snapmint.com/assets/express_checkout/Whitelabel_Images/percent_tag_img.svg" />
                                <div class="snap_coupon_code">${element.discount_code}</div>
                            </div>
                            <div class="snap_display_flex" style="display: none">
                                <img
                                    style="display:none; visibility:hidden"
                                    src="https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_code_applied_icon.svg"
                                />
                                <div class="snap_coupon_code_applied_msg" style="display:none;">
                                    <span class="snap_font_weight_bold snap_padding_right"><div class="snap_coupon_code">${element.discount_code}</div></span>Applied
                                </div>
                            </div>
                            <div class="snap_coupon_not_applicable_msg snap_applicable_error_msg_position" style="display: none">
                                Coupon Not Applicable
                            </div>
                            <div class="snap_coupon_tkt_info_txt">${element.description}</div>
                        </div>
                        <div class="snap_tkt_right_section" style="display:flex">
                           <img src="https://assets.snapmint.com/assets/express_checkout/Whitelabel_Images/tick_mark_coupons.svg"  class="snap_tkt_right_section_img" style="cursor:pointer;display:none">
                            <div
                                class="snap_coupon_apply_btn snap_cursor_pointer Apply"
                                id=${element.discount_code}
                            >
                                Apply
                            </div>
                        </div>
                    </div>`;
                        coupens.push(coups);
                        document.querySelector('.coupenDiv').innerHTML = coupens.join('');
                    });
                }
                var applyBtn = document.querySelectorAll('.snap_coupon_apply_btn');
                if (applyBtn) {
                    var listLength = 0;
                    applyBtn.forEach(function (l) {
                        if (l.id) {
                            listLength++;
                        }
                    });
                }
                if (applyBtn) {
                    let btnClick = false;
                    applyBtn.forEach(function (l) {
                        if (l.classList.contains('inputApply')) {
                            l.style.pointerEvents = 'auto';
                        } else {
                            l.style.pointerEvents = 'none';
                        }
                        l.addEventListener('click', buttonPressed);
                        if (checkCouponApplied(event, l.id, listLength)) {
                            l.click();
                            btnClick = true;
                        }
                    });
                    if (!btnClick && listLength > 0) {
                        let applyCoupoun = true;
                        if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
                            if (onWebsiteCouponApplied) {
                                applyCoupoun = false;
                            }
                        }
                        let applied_Coupon = document.querySelector('.coupon_applied_already');
                        if (applied_Coupon && applyCoupoun) {
                            let coupoun = applied_Coupon.classList[applied_Coupon.classList.length - 1];
                            if (coupoun && document.querySelector('.snap_coupon_input')) {
                                document.querySelector('.snap_coupon_input').value = coupoun.toUpperCase();
                                if (document.querySelector('.snap_coupon_apply_btn')) {
                                    document.querySelector('.snap_coupon_apply_btn').click();
                                }
                            }
                        }
                    }
                }
                var coupenDIv = document.querySelectorAll('.snap_coupon_tkt');
                if (coupenDIv) {
                    coupenDIv.forEach(function (j) {
                        j.addEventListener('click', function (e) {
                            j.querySelector('.snap_tkt_right_section .snap_coupon_apply_btn').click();
                        });
                    });
                }
            }
        } catch (err) {
            console.log(err);
        }
    }
    makeRequest(event);
    if (document.querySelector('.back_button_click')) {
        document.querySelector('.back_button_click').addEventListener('click', () => {
            document.querySelector('.coupoun_discount').style.display = 'block';
            document.querySelector('.coupoun_summary_block').style.display = 'none';
            if (document.querySelector('.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error')) {
                document.querySelector('.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error').style.display =
                    'none';
                if (document.querySelector('.snap_coupon_not_applicable_msg.snap_applicable_error_msg_position'))
                    document.querySelector(
                        '.snap_coupon_not_applicable_msg.snap_applicable_error_msg_position'
                    ).style.display = 'none';
                if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
                    if (document.querySelector('.snap_coupon_not_applicable_msg.snap_applicable_error_msg_position')) {
                        let allElement = document.querySelectorAll(
                            '.snap_coupon_not_applicable_msg.snap_applicable_error_msg_position'
                        );
                        allElement.forEach((element) => {
                            element.style.display = 'none';
                        });
                    }
                }
                document.querySelector('.snap_coupon_input').classList.remove('snap_coupon_not_applicable_error');
            }
        });
    }
    function getCodeApplied(snapMintCouponList) {
        let inputVlaue = document.querySelector('.snap_coupon_input');
        let snapDiscountCode = '';
        if (snapMintCouponList && snapMintCouponList.length > 0 && inputVlaue) {
            snapMintCouponList.forEach((element) => {
                if (element.discount_code) {
                    if (inputVlaue.value.toLowerCase() === element.discount_code.toLowerCase()) {
                        snapDiscountCode = element.discount_code;
                    }
                }
            });
        }
        if (snapDiscountCode === '') {
            if (snapOptions.isTest && snapOptions.isTest !== 'undefined') snapDiscountCode = inputVlaue.value.toUpperCase();
            else snapDiscountCode = inputVlaue.value;
        }
        return snapDiscountCode;
    }
    const buttonPressed = (e) => {
    if(document.querySelector(".snap_product_price .total_order_value"))
        {
         var  ProdPrice = document.querySelector(".snap_product_price .total_order_value").innerText
            var order_value = parseNum(ProdPrice);
        }else
        {
           var order_value = snapOrderValue; 
        }
        document.querySelector('#loader').style.display = 'block';
        if (document.querySelector('.snap_applying_coupon_loader_txt'))
            document.querySelector('.snap_applying_coupon_loader_txt').textContent = 'Applying Coupon';
        if (e.target.id) var codeApplied = e.target.id;
        else var codeApplied = getCodeApplied(snapMintCouponList);
        if (e.target.classList.contains('inputApply') && !codeApplied) {
            var codeApplied = '';
            document.querySelector('#loader').style.display = 'none';
            document.querySelector('.snap_coupon_input').classList.add('snap_coupon_not_applicable_error');
            document.querySelector('.snap_margin_top_coupon_error').style.display = 'block';
            return true;
        }
        if (e.target.classList.contains('Remove')) {
            var codeApplied = '';
            document.querySelector('.snap_applying_coupon_loader_txt').textContent = 'Removing Coupon';
            document.querySelector('.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error').style.display =
                'none';
            let code = snapDiscountCode;
            snapDiscountCode = '';
            discountedOrderValue = '';
            document.querySelector('.snap_coupon_input').classList.remove('snap_coupon_applied_edit');
            if (getByIdOrClassName(snapOptions.miniCartDivId)) {
                if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
                    addminCartLoadPage();
                } else {
                    MiniCartOpen();
                }
            } else {
                loadOnPage();
            }
            window.localStorage.removeItem('currentDiscountCode');
            changeValueAccordingToCoupon(snapOrderValue);
            order_value = orderValueNotChange;
            if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
                removeCoupon(code);
            }
            code = '';
        }
        document.querySelector('.modal-wrpr.plan_popup').classList.remove('discounted');
        document.querySelector('.modal-wrpr.discount').classList.remove('discountAdded');
        var applyBtntext = document.querySelectorAll('.snap_coupon_apply_btn');
        if (applyBtntext) {
            applyBtntext.forEach(function (k) {
                k.classList.remove('Remove');
                k.textContent = 'Apply';
                k.classList.add('Apply');
            });
        }

        document.querySelector('.snap_coupon_info_section .snap_delivery_charges_text').style.display = 'flex';

        var applyBtntext2 = document.querySelectorAll('.snap_coupon_tkt_info');
        if (applyBtntext2) {
            applyBtntext2.forEach(function (k) {
                k.classList.remove('snap_cupncode_applied');
            });
        }

        document.querySelectorAll('.snap_tkt_right_section img').forEach(function (k) {
            k.src = 'https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_tkt-right_part.svg';
        });
        if (snapOptions.newDiscountSummary && snapOptions.newDiscountSummary !== 'undefined') {
            document.querySelectorAll('.snap_tkt_right_section img').forEach(function (k) {
                k.src = '';
            });
        }
        document.querySelectorAll('.snap_coupon_applied_tkt_bg').forEach(function (k) {
            k.classList.remove('snap_coupon_applied_tkt_bg');
        });
        document.querySelectorAll('.snap_coupon_tkt_info .snap_applicable_error_msg_position').forEach(function (k) {
            k.style.display = 'none';
        });
        document.querySelectorAll('.snap_display_flex').forEach(function (k) {
            k.style.display = 'none';
        });

        document.querySelectorAll('.snap_coupon_code').forEach(function (k) {
            k.style.display = 'block';
        });
        document
            .querySelector('.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn')
            .classList.remove('Remove');
        document.querySelector('.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn').classList.add('Apply');
        document.querySelector('.modal-wrpr.plan_popup').classList.remove('discounted');
        document.querySelector('.applied_strip').style.display = 'none';
        document.querySelectorAll('.snap_coupon_applied_section').forEach((el) => (el.style.display = 'none'));
        document.querySelector('.snap_coupon_input').value = '';
        document.querySelector('#AppliedCoupen').value = '';
        var minforDelivery = snapOptions.minForDelivery;
        if (snapOptions.minForDelivery && parseInt(order_value) < parseInt(minforDelivery)) {
            var delivary_fees = snapOptions.deliveryAmount;
            deliveryFeeSection();
            document.querySelector('.modal-wrpr.plan_popup').classList.remove('no-delivrery');
        } else {
            delivary_fees = 0;
            let removeDelivery = document.querySelector('.snap_deliveryfee_section');
            for (let i = 0; i < removeDelivery.length; i++) {
                removeDelivery[i].style.display = 'none';
            }
            totalOrderValue(order_value);
            document.querySelector('.snap_coupon_info_section .snap_delivery_charges_text').style.display = 'none';
            document.querySelector('.applied_strip .snap_delivery_charges_text').style.display = 'none';
            document.querySelector('.modal-wrpr.plan_popup').classList.add('no-delivrery');
        }
        document.querySelector('#Order_value').value = order_value;
        var myHeaders = new Headers();
        myHeaders.append('Content-Type', 'application/json');
        let elemqty = snapQuantity;
        var fprice = order_value;
        var queryString = window.location.search;
        var urlParams = new URLSearchParams(queryString);
        var variant = urlParams.get('variant');
        if (variant) {
            var fvariant = variant;
        } else {
            var p = document.querySelector(`${snapOptions.variantDivId} select[name="id"]`);
            if (p == null) {
                var p = document.querySelector(`${snapOptions.variantDivId} input[name="id"]`);
            }
            if (p) var fvariant = p.value;
        }
        if (document.querySelector(snapOptions.productNameDivId)) {
            snapProdDetail = {};
            snapProdDetail.title = document.querySelector(snapOptions.productNameDivId).innerText;
        }
        if (window.location.href.includes(snapOptions.cartUrl)) {
            elemqty = miniCartQuantity;
        }
        if (getByIdOrClassName(snapOptions.miniCartDivId)) {
            elemqty = miniCartQuantity;
        }
        if (snapOptions.isTest && snapOptions.isTest !== 'undefined') {
           
                snapProdDetail = {};
                snapProdDetail.title = '';
        }
        var raw = JSON.stringify({
            mid: snapOptions.mid,
            discount_code: codeApplied,
            order_details: {
                products: [
                    {
                        name: snapProdDetail.title,
                        productId: fvariant,
                        quantity: elemqty,
                        unitPrice: fprice
                    }
                ],
                order_total: parseInt(order_value) + parseInt(delivary_fees)
            }
        });
        let isProductsAvailable = false;
        if (snapOptions.checkUsingProduct && snapOptions.checkUsingProduct !== 'undefined') {
            if (snapProducts) {
                isProductsAvailable = true;
            }
        }
        if (
            window.location.href.includes(snapOptions.cartUrl) ||
            getByIdOrClassName(snapOptions.miniCartDivId) ||
            isProductsAvailable
        ) {
            var raw = JSON.stringify({
                mid: snapOptions.mid,
                discount_code: codeApplied,
                order_details: {
                    products: snapProducts.map((product) => ({
                        name: product.name,
                        productId: product.imei1,
                        quantity: product.quantity,
                        unitPrice: product.unit_price
                    })),
                    order_total: parseInt(order_value) + parseInt(delivary_fees)
                }
            });
        }
        var requestOptions = {
            method: 'POST',
            headers: myHeaders,
            body: raw,
            redirect: 'follow'
        };
        let fetchRequest;
        if(snapOptions.couponAppliedDetails && snapOptions.couponAppliedDetails !== 'undefined'){
            eval(snapOptions.couponAppliedDetails);
        }else{
            fetchRequest= fetch('https://api.snapmint.com/v1/shopify_plugin/check_discount', requestOptions);
        }
        fetchRequest
            .then((response) => response.json())
            .then((result) =>
                setTimeout(() => {
                    document.querySelector('#loader').style.display = 'none';
                    if (result.status == 'Success' && result.data.discounted_total && result.data.order_total) {
                        if (
                            parseInt(result.data.order_total) === parseInt(result.data.discounted_total) ||
                            parseInt(result.data.discounted_total) === parseInt(snapOrderValue)
                        ) {
                            result = {
                                message: 'Invalid discount code',
                                status: 'Error'
                            };
                        }
                    }
                    if (result.status == 'Success' && result.data.discounted_total) {
                        var removeClick = document.querySelector('.coupon_remove');
                        if (removeClick && removeClick !== undefined) {
                            removeClick.addEventListener('click', function (event) {
                                removeButton = document.querySelector('.Remove');
                                if (removeButton && removeButton !== undefined) {
                                    removeButton.click();
                                }
                            });
                        }
                        let discountedValue = 0;
                        if (result.data.discount_codes && result.data.discount_codes.length > 0) {
                            let discounted = result.data.discount_codes[0];
                            codeApplied = codeApplied;
                            discountedValue = discounted.amount;
                        }
                        document
                            .querySelectorAll('.snap_coupon_applied_section')
                            .forEach((el) => (el.style.display = 'flex'));
                        document
                            .querySelectorAll('.snap_coupon_applied_txt .snap_font_weight_bold')
                            .forEach((el) => (el.textContent = "'" + codeApplied + "'"));
                        document
                            .querySelectorAll('.snap_coupon_applied_section .snap_coupon_applied_value')
                            .forEach((el) => (el.textContent = '- â‚¹' + Math.round(discountedValue)));
                        document.querySelector('.snap_cashback_label').textContent =
                            'â‚¹' + Math.round(discountedValue) + ' saved';
                        document.querySelector('.snap_coupon_strip_right span').textContent =
                            'â‚¹' + Math.round(result.data.discounted_total + parseInt(delivary_fees));
                        document.querySelector('.snap_strikethrouh').textContent =
                            'â‚¹' + Math.round(parseInt(result.data.order_total) + parseInt(delivary_fees));
                        totalOrderValue(Math.round(parseInt(result.data.discounted_total) + parseInt(delivary_fees)));
                        document.querySelector(
                            '.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error'
                        ).style.display = 'none';
                        document
                            .querySelector('.snap_coupon_input')
                            .classList.remove('snap_coupon_not_applicable_error');
                        var tktDiv = document.querySelector('.' + CSS.escape(codeApplied) + ' .snap_coupon_tkt_info');
                        if (tktDiv) {
                            tktDiv.classList.add('snap_cupncode_applied');
                        }
                        var GrayImage = document.querySelector(
                            '.' + CSS.escape(codeApplied) + ' .snap_tkt_right_section img'
                        );
                        if (GrayImage) {
                            GrayImage.src =
                                'https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_tkt_right_section_grery.svg';

                            document.querySelector(
                                '.' + CSS.escape(codeApplied) + ' .snap_display_flex'
                            ).style.display = 'flex';
                            document.querySelector('.' + CSS.escape(codeApplied) + ' .snap_coupon_code').style.display =
                                'none';
                            document.querySelector(
                                '.' + CSS.escape(codeApplied) + ' .snap_display_flex .snap_font_weight_bold'
                            ).textContent = "'" + codeApplied + "'";

                            document.getElementById(codeApplied).textContent = 'Remove';
                            document.getElementById(codeApplied).classList.add('Remove');
                            document.getElementById(codeApplied).classList.remove('Apply');
                        }
                        if (snapOptions.newDiscountSummary && snapOptions.newDiscountSummary !== 'undefined') {
                            let checkList = document.querySelectorAll('.snap_coupon_tkt');
                            let codeAvailable = false;
                            for (let i = 0; i < checkList.length; i++) {
                                if (checkList[i].classList.contains(codeApplied)) {
                                    codeAvailable = true;
                                }
                            }
                            if (codeAvailable) {
                                document.querySelector(
                                    '.' + CSS.escape(codeApplied) + ' .snap_coupon_code'
                                ).style.display = 'block';
                                GrayImage.src =
                                    'https://assets.snapmint.com/assets/express_checkout/Whitelabel_Images/tick_mark_coupons.svg';
                                GrayImage.style.display = 'flex';
                                document.getElementById(codeApplied).textContent = 'Applied';
                                document.getElementById(codeApplied).classList.add('Remove');
                                document.getElementById(codeApplied).classList.remove('Apply');
                                document
                                    .querySelector('.snap_coupon_tkt' + '.' + CSS.escape(codeApplied))
                                    .classList.add('snap_coupon_applied_tkt_bg');
                            }
                        }
                        document.querySelector('.snap_coupon_input').textContent = codeApplied;
                        document.querySelector('.snap_coupon_input').classList.add('snap_coupon_applied_edit');
                        document.querySelector('#AppliedCoupen').value = codeApplied;
                        document.querySelector('#Order_value').value = Math.round(result.data.discounted_total);
                        document.querySelector('.snap_coupon_input').value = codeApplied;
                        document.querySelector(
                            '.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn'
                        ).textContent = 'Remove';
                        document
                            .querySelector('.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn')
                            .classList.add('Remove');
                        document
                            .querySelector('.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn')
                            .classList.remove('Apply');
                        document.querySelector('.applied_strip').style.display = 'block';
                        document.querySelector('.modal-wrpr.plan_popup').classList.add('discounted');
                        document.querySelector('.modal-wrpr.discount').classList.add('discountAdded');
                        if (!getDynamicJsCookie('snapmintDynamicJscookie')) {
                            document.querySelector('.modal-wrpr.plan_popup').style.display = 'block';
                        }
                        if (document.querySelector('.snap_display_flex_input')) {
                            document.querySelector('.snap_display_flex_input').style.display = 'flex';
                            document.querySelector('.snap_display_flex_input .code-applied').textContent =
                                "'" + codeApplied + "'";
                        }
                        snapOrderValue = Math.round(result.data.discounted_total);
                        discountedOrderValue = Math.round(result.data.discounted_total);
                        snapDiscountCode = codeApplied;
                        changeValueAccordingToCoupon(discountedOrderValue);
                        addingSubtoalValue();
                        document.querySelector('.coupoun_discount').style.display = 'block';
                        document.querySelector('.coupoun_summary_block').style.display = 'none';
                    } else if (result.status == 'Success' && !result.data.discounted_total) {
                        var Discounted_amount = parseNum(result.data.discount_codes[0].amount);
                        codeApplied = result.data.discount_codes[0].code;
                        document
                            .querySelectorAll('.snap_coupon_applied_section')
                            .forEach((el) => (el.style.display = 'flex'));
                        document
                            .querySelectorAll('.snap_coupon_applied_txt .snap_font_weight_bold')
                            .forEach((el) => (el.textContent = "'" + codeApplied + "'"));
                        document.querySelector('.snap_coupon_applied_section .snap_coupon_applied_value').textContent =
                            '- â‚¹' + Math.round(Discounted_amount);
                        document.querySelector('.snap_cashback_label').textContent = 'â‚¹' + Discounted_amount + ' saved';
                        document.querySelector('.snap_coupon_strip_right span').textContent =
                            'â‚¹' +
                            Math.round(parseInt(order_value) - parseInt(Discounted_amount) + parseInt(delivary_fees));
                        document.querySelector('.snap_strikethrouh').textContent =
                            'â‚¹' + Math.round(parseInt(order_value) + parseInt(delivary_fees));
                        totalOrderValue(
                            Math.round(parseInt(order_value) - parseInt(Discounted_amount) + parseInt(delivary_fees))
                        );
                        document.querySelector(
                            '.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error'
                        ).style.display = 'none';
                        document
                            .querySelector('.snap_coupon_input')
                            .classList.remove('snap_coupon_not_applicable_error');
                        var tktDiv = document.querySelector('.' + CSS.escape(codeApplied) + ' .snap_coupon_tkt_info');

                        if (tktDiv) {
                            tktDiv.classList.add('snap_cupncode_applied');
                        }
                        document.querySelector(
                            '.' + CSS.escape(codeApplied) + ' .snap_display_flex .snap_font_weight_bold'
                        ).textContent = "'" + codeApplied + "'";
                        var GrayImage = document.querySelector(
                            '.' + CSS.escape(codeApplied) + ' .snap_tkt_right_section img'
                        );
                        if (GrayImage) {
                            GrayImage.src =
                                'https://preemi.snapmint.com/assets/express_checkout/Discount_Images/coupon_tkt_right_section_grery.svg';
                            document.querySelector('.snap_coupon_input').textContent = codeApplied;
                            document.querySelector(
                                '.' + CSS.escape(codeApplied) + ' .snap_display_flex'
                            ).style.display = 'flex';
                            document.querySelector('.' + CSS.escape(codeApplied) + ' .snap_coupon_code').style.display =
                                'none';
                            document.getElementById(codeApplied).textContent = 'Remove';
                            document.getElementById(codeApplied).classList.add('Remove');
                            document.getElementById(codeApplied).classList.remove('Apply');
                        }
                        if (snapOptions.newDiscountSummary && snapOptions.newDiscountSummary !== 'undefined') {
                            let checkList = document.querySelectorAll('.snap_coupon_tkt');
                            let codeAvailable = false;
                            for (let i = 0; i < checkList.length; i++) {
                                if (checkList[i].classList.contains(codeApplied)) {
                                    codeAvailable = true;
                                }
                            }
                            if (codeAvailable) {
                                GrayImage.src =
                                    'https://assets.snapmint.com/assets/express_checkout/Whitelabel_Images/tick_mark_coupons.svg';
                                GrayImage.style.display = 'flex';
                                document.querySelector(
                                    '.' + CSS.escape(codeApplied) + ' .snap_coupon_code'
                                ).style.display = 'block';
                                document
                                    .querySelector('.' + CSS.escape(codeApplied))
                                    .classList.add('snap_coupon_applied_tkt_bg');
                                document.getElementById(codeApplied).textContent = 'Applied';
                                document.getElementById(codeApplied).classList.add('Remove');
                                document.getElementById(codeApplied).classList.remove('Apply');
                            }
                        }
                        document.querySelector('.snap_coupon_input').value = codeApplied;
                        document.querySelector('#AppliedCoupen').value = codeApplied;
                        document.querySelector('#Order_value').value =
                            parseInt(order_value) - parseInt(Discounted_amount);
                        document.querySelector(
                            '.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn'
                        ).textContent = 'Remove';
                        document
                            .querySelector('.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn')
                            .classList.add('Remove');
                        document
                            .querySelector('.snap_apply_coupon_section .div-wrap .snap_coupon_apply_btn')
                            .classList.remove('Apply');
                        document.querySelector('.applied_strip').style.display = 'block';
                        document.querySelector('.modal-wrpr.plan_popup').classList.add('discounted');
                        document.querySelector('.modal-wrpr.discount').classList.add('discountAdded');
                        document.querySelector('.modal-wrpr.plan_popup').style.display = 'block';
                        document.querySelector('.snap_coupon_info_section .snap_delivery_charges_text').style.display =
                            'none';
                        if (document.querySelector('.snap_display_flex_input')) {
                            document.querySelector('.snap_display_flex_input').style.display = 'flex';
                            document.querySelector('.snap_display_flex_input .code-applied').textContent =
                                "'" + codeApplied + "'";
                        }
                        snapOrderValue = parseInt(order_value) - parseInt(Discounted_amount);
                        discountedOrderValue = parseInt(order_value) - parseInt(Discounted_amount);
                        snapDiscountCode = codeApplied;
                        changeValueAccordingToCoupon(discountedOrderValue);
                        addingSubtoalValue();
                    } else {
                        let priceTag = '';
                        if (getByIdOrClassName(snapOptions.priceSaleDivId)) {
                            priceTag = getByIdOrClassName(snapOptions.priceSaleDivId);
                        } else {
                            priceTag = getByIdOrClassName(snapOptions.priceDivId);
                        }
                        if (
                            snapOptions.findAmount &&
                            snapOptions.findAmount !== 'undefined' &&
                            !window.location.href.includes(snapOptions.cartUrl)
                        ) {

                            snapOrderValue = parseNum(eval(snapOptions.findAmount)) * snapQuantity+parseInt(delivary_fees);
                        } else {
                               if(priceTag){
                    snapOrderValue = parseNum(priceTag.innerText) * snapQuantity + parseInt(delivary_fees);
                }else
                {
                    snapOrderValue=productdesc.snapOrderValue;
                }

                        }

                        let order_value = snapOrderValue;
                        if (
                            snapOptions.checkUsingProduct &&
                            snapOptions.checkUsingProduct !== 'undefined' &&
                            productdesc
                        ) {
                            order_value = parseInt(productdesc.snapOrderValue);
                        }
                        document
                            .querySelectorAll('.snap_coupon_applied_section')
                            .forEach((el) => (el.style.display = 'none'));
                        totalOrderValue(parseInt(order_value));
                        addingDeliveryCharge();
                        addingSubtoalValue();
                        if (document.querySelector('.snap_display_flex_input')) {
                            document.querySelector('.snap_display_flex_input').style.display = 'none';
                        }
                        document.querySelector('#Order_value').value = order_value;
                        if (e.target.classList.contains('inputApply') && codeApplied) {
                            document
                                .querySelector('.snap_coupon_input')
                                .classList.add('snap_coupon_not_applicable_error');
                            document.querySelector(
                                '.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error'
                            ).style.display = 'block';
                            return true;
                        }
                        if (codeApplied) {
                            document
                                .querySelector('.snap_coupon_input')
                                .classList.add('snap_coupon_not_applicable_error');
                            document.querySelector(
                                '.' +
                                    CSS.escape(codeApplied) +
                                    ' .snap_coupon_tkt_info .snap_applicable_error_msg_position'
                            ).style.display = 'block';
                            document
                                .querySelector('.' + CSS.escape(codeApplied) + ' .snap_coupon_tkt_info_txt')
                                .classList.add('snap_ellipsis_txt');
                            if (document.querySelector('.snap_display_flex_input')) {
                                document.querySelector('.snap_display_flex_input').style.display = 'none';
                            }
                            document.querySelector(
                                '.' + CSS.escape(codeApplied) + ' .snap_display_flex'
                            ).style.display = 'none';
                            document.querySelector('.' + CSS.escape(codeApplied) + ' .snap_coupon_code').style.display =
                                'block';
                            document.querySelector(
                                '.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error'
                            ).style.display = 'block';
                        } else {
                            if (document.querySelector('.snap_display_flex_input')) {
                                document.querySelector('.snap_display_flex_input').style.display = 'none';
                            }
                            document.querySelector(
                                '.snap_coupon_not_applicable_msg.snap_margin_top_coupon_error'
                            ).style.display = 'none';
                        }
                    }
                }, 2000)
            );
    };

    if (document.querySelector('#lname')) {
        document.querySelector('#lname').addEventListener('mouseover', function (evt) {
            document.querySelector('.snap_lastname_popup').style.display = 'block';
        });
    }

    if (document.querySelector('#lname')) {
        document.querySelector('#lname').addEventListener('mouseout', function (evt) {
            document.querySelector('.snap_lastname_popup').style.display = 'none';
        });
    }

    if (document.querySelector('#fname')) {
        document.querySelector('#fname').addEventListener('mouseover', function (evt) {
            document.querySelector('.snap_firstname_popup').style.display = 'block';
        });
    }

    if (document.querySelector('#fname')) {
        document.querySelector('#fname').addEventListener('mouseout', function (evt) {
            document.querySelector('.snap_firstname_popup').style.display = 'none';
        });
    }
}
function openCouponPage() {
    document.querySelector('.coupoun_discount').style.display = 'none';
    document.querySelector('.coupoun_summary_block').style.display = 'block';
    if (snapOptions.deliveryAmount && snapOptions.deliveryAmount !== 'undefined') {
        if (snapOptions.minForDelivery && parseInt(snapOrderValue) < parseInt(snapOptions.minForDelivery)) {
            deliveryFeeSection();
        }
    }
    removeErrorMessages();
}

function getAutoMaticCoupon(snapOrderValue) {
    //find coupon code on the basis of order value and return coupon code
    let couponCode = '';
    if (snapOptions.autoMaticCoupon && snapOptions.autoMaticCoupon !== 'undefined') {
        let couponArray = JSON.parse(snapOptions.autoMaticCoupon);
        if (couponArray && couponArray.length > 0) {
            couponArray.forEach((element) => {
                if (snapOrderValue >= element.min && snapOrderValue <= element.max) {
                    couponCode = element.code;
                }
            });
        }
    }
    return couponCode;
}
function addClassForAutoMaticCoupon(snapOrderValue, isMiniCart = false) {
    let coupon = getAutoMaticCoupon(snapOrderValue);
    if (!coupon) {
        coupon = JSON.stringify(window.localStorage.getItem('currentDiscountCode'));
        coupon = JSON.parse(coupon);
    }
    if (coupon && snapOptions.particularCoupon && snapOptions.particularCoupon !== 'undefined') {
        coupon = eval(snapOptions.particularCoupon);
    }
    if (coupon) {
        let element = getByIdOrClassName('#sm-widget-btn');
        if (document.querySelector('.second_widget_btn')) element = document.querySelector('.second_widget_btn');
        if (isMiniCart) {
            element = getByIdOrClassName('.snap_emi_txt_minicart');
        }
        if (element) {
            if (!element.classList.contains('coupon_applied_already')) {
                element.classList.add('coupon_applied_already');
                element.classList.add(coupon);
            }
        }
    }
}function addingInputFieldKeyup() {
    if (document.querySelector('#fname')) {
        document.querySelector('#fname').addEventListener('click', function(evt) {
            if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
                captureEvents('2.0_first_name_clicked', {
                    merchant_id: snapOptions.mid,
                    merchant_name: snapOptions.merchantName
                });
            }
        });
      document.querySelector('#fname').addEventListener('keyup', function (evt) {
            var fname = document.querySelector('#fname').value;
            document.querySelector('#fname').setAttribute('maxLength',25);
           document.querySelector('#fname').value = fname.replace(/[^a-zA-Z]/g, '').replace(/(\..*)\./g, '$1');
            if (fname == '') {
                document.getElementById('err_fname').innerText = 'Please Check First Name';
                return false;
            } else {
                document.getElementById('err_fname').innerHTML = '';
                document.getElementById('fname').classList.remove('error_field');
                return false;
            }
        });
    }

    if (document.querySelector('#lname')) {
        document.querySelector('#lname').addEventListener('click', function(evt) {
            if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
                captureEvents('2.0_last_name_clicked', {
                    merchant_id: snapOptions.mid,
                    merchant_name: snapOptions.merchantName
                });
            }
        });
        document.querySelector('#lname').addEventListener('keyup', function(evt) {
            var lname = document.querySelector('#lname').value;
            document.querySelector('#lname').setAttribute('maxLength',25);
            document.querySelector('#lname').value = lname.replace(/[^a-zA-Z]/g, '').replace(/(\..*)\./g, '$1');
            var fname = document.querySelector('#fname').value;
            if (fname == '') {
                document.getElementById('err_fname').innerHTML = 'Please Check First Name';
            }
            if (lname == '') {
                document.getElementById('err_lname').innerHTML = 'Please Check Last Name';
                return false;
            } else {
                document.getElementById('err_lname').innerHTML = '';
                document.getElementById('lname').classList.remove('error_field');
                return false;
            }
        });
    }
    if (document.querySelector('#mnumber')) {
        document.querySelector('#mnumber').addEventListener('click', function(evt) {
            if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
                captureEvents('2.0_phone_number_clicked', {
                    merchant_id: snapOptions.mid,
                    merchant_name: snapOptions.merchantName
                });
            }
        });
        document.querySelector('#mnumber').addEventListener('keyup', function(evt) {

            var firstChar = document.querySelector('#mnumber').value.charAt(0);
if( firstChar <='5' && firstChar >='0') {
            var phoneNumber = document.querySelector('#mnumber').value;
     document.querySelector('#mnumber').value = phoneNumber.replace(/[0-5]/g, '').replace(/(\..*)\./g, '$1');
}
            if (evt.keyCode < 48 || evt.keyCode > 57) {
                var phoneNumber = document.querySelector('#mnumber').value;
                document.querySelector('#mnumber').value = phoneNumber.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');
            }
            if (document.querySelector('#fname')) {
                var fname = document.querySelector('#fname').value;
                if (fname == '') {
                    document.getElementById('err_fname').innerHTML = 'Please Check First Name';
                }
                var lname = document.querySelector('#lname').value;
                if (lname == '') {
                    document.getElementById('err_lname').innerHTML = 'Please Check Last Name';
                }
            }
        });
    }
}
function fetchDataForShopify(minCartLoadPage=false) {
    let delivery_amount = 0;
    //adding delivery charge for express
    if (snapOptions.addingDeliveryAmountExpress && snapOptions.addingDeliveryAmountExpress !== 'undefined') {
        delivery_amount = parseInt(snapOptions.addingDeliveryAmountExpress);
    }
    if(snapOptions.merchantCartFetch && snapOptions.merchantCartFetch !== 'undefined'){
         eval(snapOptions.merchantCartFetch);
         return;
    }
    fetch(window.Shopify.routes.root + 'cart.js')
        .then((response) => response.json())
        .then((data) => {
            if (data.total_price) {
                if(snapOptions.isSubtotalVisible && snapOptions.isSubtotalVisible !== 'undefined')
                snapOptions.isSubtotalVisible = parseInt(data.total_price/100);
                if(minCartLoadPage){
                    addminCartLoadPage();
                }
                miniCartQuantity = data.item_count;
                if (snapOptions.deliveryAmount && snapOptions.deliveryAmount !== 'undefined') {
                    delivery_amount = addDeliveryAmouunt(data.item_count);
                }
                productdesc = {
                    snapOrderValue: data.total_price / 100 + delivery_amount * data.item_count,
                    merchantId: snapOptions.mid
                };
                snapProducts.clear();
                for (i = 0; i < data.items.length; i++) {
                    var items = {
                        sku: '',
                        name: data.items[i].product_title,
                        quantity: data.items[i].quantity,
                        unit_price: data.items[i].final_price / 100,
                        imei1: data.items[i].variant_id,
                        source: 'cart'
                    };
                    const gmtProps= {
                        ...items,
                        product_id: data.items[i].product_id,
                        image_url: data.items[i].image,
                        product_url: data.items[i].url,
                    }
                    if (snapOptions.personaliseDivId && snapOptions.personaliseDivId !== 'undefined') {
                        if (data.items[i].properties) {
                            items['product_comment'] = data.items[i].properties;
                        }
                    }
                    snapProducts.push(items);
                    webEngageInfo.push(gmtProps);
                    if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
                        if (data.items[i].discounts && data.items[i].discounts.length > 0) {
                            onWebsiteCouponApplied = data.items[i].discounts[0].title;
                        } else {
                            onWebsiteCouponApplied = null;
                        }
                    }
                }
            }
            handleServiceWorker(snapProducts, productdesc, 'checkcokiee');
        });
}

function createCartFromCartPage() {
    fetchDataForShopify();
}
function createCart() {
    snapProducts.clear();
    webEngageInfo.clear();
    let elemqty = 1;
    if (window.location.href.includes(snapOptions.cartUrl)) {
        createCartFromCartPage();
        isMiniCartPopupOpen = false;
    } else {
        let mini_cart = getByIdOrClassName(snapOptions.miniCartDivId);
        if (mini_cart) {
            isMiniCartPopupOpen = true;
            createCartFromMiniCart();
        } else {
            isMiniCartPopupOpen = false;
            if (snapQuantity) {
                elemqty = snapQuantity;
            }
            var fprice = '';
            var priceF = '';
            if (snapOptions.findAmount &&snapOptions.findAmount !== 'undefined' ) {
				priceF = eval(snapOptions.findAmount);
			} else {
			   if (getByIdOrClassName(snapOptions.priceSaleDivId)) {
						priceF = getByIdOrClassName(snapOptions.priceSaleDivId).innerText;
				} else {
						priceF = getByIdOrClassName(snapOptions.priceDivId).innerText;
				}
			}
            fprice = parseNum(priceF);
            var name = getByIdOrClassName(snapOptions.productNameDivId).innerText;
            var queryString = window.location.search;
            var urlParams = new URLSearchParams(queryString);
            var variant = urlParams.get('variant');
            if (variant) {
                var fvariant = variant;
            } else {
                if (snapOptions.findVariantId.length > 0 && snapOptions.findVariantId !== 'undefined') {
                    var fvariant = eval(snapOptions.findVariantId);
                } else {
                    var e = document.querySelector(`${snapOptions.variantDivId} select[name="id"]`);
                    if (e == null) {
                        var e = document.querySelector(`${snapOptions.variantDivId} input[name="id"]`);
                    }
                    if (e) var fvariant = e.value;
                    else addOutOfStockError();
                }
            }
            var items = {
                sku: '',
                name: name,
                quantity: elemqty,
                unit_price: fprice,
                imei1: fvariant,
                source: 'pdp'
            };
            snapProducts.push(items);
            const gmtProps= {
                ...items,
                product_id: eval(snapOptions.productId),
                image_url: eval(snapOptions.productImgUrl),
                product_url: window.location.href,
            }
            webEngageInfo.push(gmtProps);
            //adding delivery charge for express
            let delivery_amount = 0;
            if (snapOptions.addingDeliveryAmountExpress && snapOptions.addingDeliveryAmountExpress !== 'undefined') {
                delivery_amount = parseInt(snapOptions.addingDeliveryAmountExpress);
            }
            if (snapOptions.deliveryAmount && snapOptions.deliveryAmount !== 'undefined') {
                delivery_amount = addDeliveryAmouunt(elemqty);
            }
            productdesc = {
                snapOrderValue: elemqty * (delivery_amount + fprice),
                merchantId: snapOptions.mid
            };
            handleServiceWorker(snapProducts, productdesc, 'checkcokiee');
        }
    }
}
function checkOutOfStock() {
    if (getByIdOrClassName(snapOptions.checkoutBtnId)) {
        let add_Cart_btn = getByIdOrClassName(snapOptions.checkoutBtnId).innerText;
        let isCheckOutOfStock = false;
        if (snapOptions.checkSoldOutProduct && snapOptions.checkSoldOutProduct !== 'undefined') {
            let element = getByIdOrClassName(snapOptions.checkSoldOutProduct);
            if (element && element.innerText.toLowerCase() === snapOptions.outOfStock) {
                isCheckOutOfStock = true;
            }
            if (snapOptions.productNotAvailable && snapOptions.productNotAvailable !== 'undefined') {
                let element = getByIdOrClassName(snapOptions.checkSoldOutProduct);
                if (element && element.innerText.toLowerCase() === snapOptions.productNotAvailable) {
                    isCheckOutOfStock = true;
                }
            }
        }
        if (snapOptions.checkDisableCheckout && snapOptions.checkDisableCheckout !== 'undefined') {
            if (add_Cart_btn === '') {
                add_Cart_btn = 'check disable checkout btn';
            }
        }
        if (add_Cart_btn) {
            let isProductOutOfStockAvailable = false;
            try {
                if (snapProdDetail && snapProdDetail.available) {
                    if (snapProdDetail.available === false) {
                        isProductOutOfStockAvailable = true;
                    }
                }
            } catch (e) {}
            if (snapOptions.checkVariant && snapOptions.checkVariant !== 'undefined') {
                var queryString = window.location.search;
                var urlParams = new URLSearchParams(queryString);
                var variant = urlParams.get('variant');
                if (!variant) {
                    isProductOutOfStockAvailable = true;
                }
            }
            if (snapOptions.checkDisableCheckout && snapOptions.checkDisableCheckout !== 'undefined') {
                if (document.querySelector(snapOptions.checkDisableCheckout)) {
                    isProductOutOfStockAvailable = true;
                }
            }
            if(snapOptions.outOfStockTrim && snapOptions.outOfStockTrim !== 'undefined'){
                add_Cart_btn = add_Cart_btn.trim();
            }
            if (
                add_Cart_btn.toLowerCase() === 'out of stock' ||
                add_Cart_btn.toLowerCase() === snapOptions.outOfStock ||
                isCheckOutOfStock ||
                isProductOutOfStockAvailable
            ) {
                addOutOfStockError();
            }
        }
    }
}

async function handlesubmit(productdesc) {
    if (document.getElementById('fname')) var fname = document.getElementById('fname').value;
    if (document.getElementById('lname')) var lname = document.getElementById('lname').value;
    var phoneNumber = document.getElementById('mnumber').value;

    if (fname === '' || lname === '' || phoneNumber === '' || !validatePhoneNumbers(phoneNumber)) {
        if (fname === '') {
            if (document.getElementById('err_fname')) {
                document.getElementById('err_fname').innerText = 'Please Check First Name';
                document.getElementById('fname').classList.add('error_field');
            }
            return false;
        }
        if (fname != '') {
            if (document.getElementById('err_fname')) {
                document.getElementById('err_fname').innerText = '';
                document.getElementById('fname').classList.remove('error_field');
            }
        }

        if (lname === '') {
            if (document.getElementById('err_lname')) {
                document.getElementById('err_lname').innerText = 'Please Check Last Name';
                document.getElementById('lname').classList.add('error_field');
            }
            return false;
        }
        if (lname != '') {
            if (document.getElementById('err_lname')) {
                document.getElementById('err_lname').innerText = '';
                document.getElementById('lname').classList.remove('error_field');
            }
        }

        if (phoneNumber === '') {
            document.getElementById('err_num').innerText = 'Please Check Mobile Number';
            document.getElementById('mnumber').classList.add('error_field');
            return false;
        }
        if (!validatePhoneNumbers(phoneNumber)) {
            document.getElementById('err_num').innerText = 'Please Check Mobile Number';
            document.getElementById('mnumber').classList.add('error_field');
            return false;
        }
        if (phoneNumber !== '' && validatePhoneNumbers(phoneNumber)) {
            document.getElementById('err_num').innerText = '';
            document.getElementById('mnumber').classList.remove('error_field');
        }
        return false;
    } else {
        document.getElementById('mnumber').classList.remove('error_field');
        if (document.getElementById('lname')) document.getElementById('lname').classList.remove('error_field');
        if (document.getElementById('fname')) document.getElementById('fname').classList.remove('error_field');
        if (document.querySelector('.snapLoader')) {
            document.querySelector('.snapLoader').setAttribute('style', 'display:block;');
        }
        if (document.querySelector('.snapmint_lowest_emi')) {
            document.querySelector('.snapmint_lowest_emi').setAttribute('style', 'display:none;');
        }
        if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
            captureEvents('2.0_phone_submit_clicked', {
                merchant_id: snapOptions.mid,
                merchant_name: snapOptions.merchantName
            });
        }
        var orderVal = productdesc.snapOrderValue;
        var saPAttributes = getCookieAttributes('_shopify_sa_p');
        var saTAttributes = getCookieAttributes('_shopify_sa_t');
        var c_snap_landing_page = getDynamicJsCookie('snap_landing_page');
        var c_snap_referrer_url = getDynamicJsCookie('snap_referrer_url');
        var data = {
            merchant_confirmation_url: productdesc.merchant_confirmation_url,
            merchant_failure_url: productdesc.merchant_failure_url,
            merchant_id: productdesc.merchantId,
            order_id: '',
            order_value: orderVal,
            first_name: fname,
            middle_name: '',
            last_name: lname,
            email: '',
            mobile: phoneNumber,
            products: snapProducts,
            source:snapProducts[0]['source'],
            _orig_referrer: c_snap_referrer_url,
            _landing_page: c_snap_landing_page,
            _shopify_sa_p: saPAttributes.value,
            _shopify_sa_t: saTAttributes.value,
            merchant_confirmation_url: snapOptions.merchant_confirmation_url,
            merchant_failure_url: snapOptions.merchant_failure_url,
        };
        if (
            discountedOrderValue &&
            snapDiscountCode &&
            snapOptions.isCashbackCoupon &&
            snapOptions.isCashbackCoupon === 'true'
        ) {
            discountedOrderValue = Math.floor(discountedOrderValue);
            if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
                if (onWebsiteCouponApplied) {
                    data.order_value = snapOrderValue;
                } else {
                    data.order_value = discountedOrderValue;
                }
            } else {
                data.order_value = discountedOrderValue;
            }
            data['coupon'] = snapDiscountCode;
        }
        if (snapOptions.autoMaticCoupon && snapOptions.autoMaticCoupon !== 'undefined') {
            if (!data['coupon']) {
                data['coupon'] = getAutoMaticCoupon(orderVal);
            }
        }
        if (snapUnleashFlag) {
            data['udf1'] = snapUnleashFlag;
        }
        if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
            if (onWebsiteCouponApplied) {
                data['coupon'] = onWebsiteCouponApplied;
            }
        }
        if (snapOptions.shippingFee && snapOptions.shippingFee !== 'undefined') {
            let localQuantity = snapQuantity;
            if (
                window.location.href.includes(snapOptions.cartUrl) ||
                getByIdOrClassName(snapOptions.miniCartDivId) ||
                snapProducts.length > 0
            ) {
                localQuantity = 0;
                for (let i = 0; i < snapProducts.length; i++) {
                    localQuantity += snapProducts[i].quantity;
                }
            }
            let checkMinValue = '';
            if (snapOptions.minForDelivery && snapOptions.minForDelivery !== 'undefined')
                checkMinValue = snapOptions.minForDelivery;
            if (
                snapOptions.addingDeliveryMaxAmountExpress &&
                snapOptions.addingDeliveryMaxAmountExpress !== 'undefined'
            )
                checkMinValue = snapOptions.addingDeliveryMaxAmountExpress;
            if (parseInt(productdesc.snapOrderValue) < parseInt(checkMinValue)) {
                data['shipping_id'] = snapOptions.shippingId;
                if (snapOptions.shippingFeeUnit && snapOptions.shippingFeeUnit !== 'undefined') {
                    data['shipping_fee'] = snapOptions.shippingFeeUnit;
                } else {
                    data['shipping_fee'] = parseInt(snapOptions.shippingFee) * localQuantity;
                }
                data['shipping_title'] = snapOptions.shippingTitle;
            }
        }
        let resp = await getToken(data);
        if (resp.status === 'Success') {
            if(snapOptions.isGTMEnable && snapOptions.isGTMEnable !== 'undefined') {
                dataLayer.push({
                    'event': 'cart_created',
                    values : {
                        'merchant_id': productdesc.merchantId, 
                    },
                    'page_location': window.location.href,
                    'page_path': window.location.pathname,
                    'merchant_name': snapOptions.merchantName,
                    'mobile': phoneNumber,
                    'order_value': orderVal.toString(),
                    'first_name': fname,
                    'last_name': lname,
                    'orig_referrer': c_snap_referrer_url,
                    'landing_page': c_snap_landing_page,
                    'shopify_sa_p': saPAttributes.value,
                    'shopify_sa_t': saTAttributes.value
                });
            }
            webEngageEvent('Mobile_Added_Snapmint', {
                'mobile': phoneNumber,
                'order_value': orderVal.toString(),
                'first_name': fname,
                'last_name': lname,
                'promo_code': data['coupon'] ? data['coupon'] : '',
                'currency': 'INR',
            });
            var iframeUrl = resp.redirect_url;

            if (iframeUrl.includes('checkout.snapmint.com') || iframeUrl.includes('checkoutv2.snapmint.com') || iframeUrl.includes("pay.snapmint.com")) {
                if (isSnapMerchantJSMobile) {
                    windowPopup = window.open(iframeUrl, 'mozillaWindow', windowFeaturesSnapmint);
                    let registerData = {
                        merchant_id: productdesc.merchantId,
                        fname: fname,
                        lname: lname,
                        phoneNumber: phoneNumber,
                        orderVal: orderVal
                    };
                    if (
                        discountedOrderValue &&
                        snapDiscountCode &&
                        snapOptions.isCashbackCoupon &&
                        snapOptions.isCashbackCoupon === 'true'
                    ) {
                        registerData['coupon'] = snapDiscountCode;
                    }
                    handleServiceWorker(registerData, 'setcokiee');

                    var encrypted = CryptoJS.AES.encrypt(iframeUrl, encryptionKeyCryptoJs);
                    window.localStorage.setItem('snapminturl', encrypted);
                    checkpopup(windowPopup, iframeUrl);
                } else {
                    openCheckV2Iframe(iframeUrl, fname, lname, phoneNumber);
                    let registerData = {
                        merchant_id: productdesc.merchantId,
                        fname: fname,
                        lname: lname,
                        phoneNumber: phoneNumber,
                        orderVal: orderVal
                    };
                    if (
                        discountedOrderValue &&
                        snapDiscountCode &&
                        snapOptions.isCashbackCoupon &&
                        snapOptions.isCashbackCoupon === 'true'
                    ) {
                        registerData['coupon'] = snapDiscountCode;
                    }
                    handleServiceWorker(registerData, 'setcokiee');
                    handleCloseButtonIframe();
                    var encrypted = CryptoJS.AES.encrypt(iframeUrl, encryptionKeyCryptoJs);
                    window.localStorage.setItem('snapminturl', encrypted);
                }
            } else {
                windowPopup = window.open(iframeUrl, 'mozillaWindow', windowFeaturesSnapmint);
                let registerData = {
                    merchant_id: productdesc.merchantId,
                    fname: fname,
                    lname: lname,
                    phoneNumber: phoneNumber,
                    orderVal: orderVal
                };
                if (
                    discountedOrderValue &&
                    snapDiscountCode &&
                    snapOptions.isCashbackCoupon &&
                    snapOptions.isCashbackCoupon === 'true'
                ) {
                    registerData['coupon'] = snapDiscountCode;
                }
                handleServiceWorker(registerData, 'setcokiee');

                var encrypted = CryptoJS.AES.encrypt(iframeUrl, encryptionKeyCryptoJs);
                window.localStorage.setItem('snapminturl', encrypted);
                checkpopup(windowPopup, iframeUrl);

                checkAuthenticated(resp.id, resp.token);
            }
        } else {
            addOutOfStockError();
        }
    }
}

async function handleServiceWorker(registerData, productdesc, typeofdata) {
    try {
        if (typeofdata === 'checkcokiee') {
            var x = getDynamicJsCookie('snapmintDynamicJscookie');
            const isMiniCart = getByIdOrClassName(snapOptions.miniCartDivId);
            if (getByIdOrClassName(snapOptions.checkoutBtnId) && !isMiniCart && !sessionStorage.getItem('sizeslected')) {
                let add_Cart_btn = getByIdOrClassName(snapOptions.checkoutBtnId).innerText;
                if (snapOptions.checkDisableCheckout && snapOptions.checkDisableCheckout !== 'undefined') {
                    if (add_Cart_btn === '') {
                        add_Cart_btn = 'check disable checkout btn';
                    }
                }
                if (add_Cart_btn) {
                    let isCheckOutOfStock = false;
                    if (snapOptions.checkSoldOutProduct && snapOptions.checkSoldOutProduct !== 'undefined') {
                        let element = getByIdOrClassName(snapOptions.checkSoldOutProduct);
                        if (element && element.innerText.toLowerCase() === snapOptions.outOfStock) {
                            isCheckOutOfStock = true;
                        }
                        if (snapOptions.productNotAvailable && snapOptions.productNotAvailable !== 'undefined') {
                            let element = getByIdOrClassName(snapOptions.checkSoldOutProduct);
                            if (element && element.innerText.toLowerCase() === snapOptions.productNotAvailable) {
                                isCheckOutOfStock = true;
                            }
                        }
                    }
                    if (snapOptions.checkVariant && snapOptions.checkVariant !== 'undefined') {
                        var queryString = window.location.search;
                        var urlParams = new URLSearchParams(queryString);
                        var variant = urlParams.get('variant');
                        if (!variant) {
                            isCheckOutOfStock = true;
                        }
                    }
                    if (snapOptions.checkDisableCheckout && snapOptions.checkDisableCheckout !== 'undefined') {
                        if (document.querySelector(snapOptions.checkDisableCheckout)) {
                            isCheckOutOfStock = true;
                        }
                    }
                    if (
                        add_Cart_btn.toLowerCase() === 'out of stock' ||
                        add_Cart_btn.toLowerCase() === snapOptions.outOfStock ||
                        isCheckOutOfStock
                    ) {
                        addOutOfStockError();
                        return 400;
                    }
                }
            }
            if (x) {
                var decrypted = CryptoJS.AES.decrypt(x, encryptionKeyCryptoJs);
                let resp = JSON.parse(decrypted.toString(CryptoJS.enc.Utf8));
                var saPAttributes = getCookieAttributes('_shopify_sa_p');
                var saTAttributes = getCookieAttributes('_shopify_sa_t');
                var c_snap_landing_page = getDynamicJsCookie('snap_landing_page');
                var c_snap_referrer_url = getDynamicJsCookie('snap_referrer_url');

                var data = {
                    merchant_confirmation_url: productdesc.merchant_confirmation_url,
                    merchant_failure_url: productdesc.merchant_failure_url,
                    merchant_id: resp['merchant_id'],
                    order_id: '',
                    order_value: productdesc.snapOrderValue,
                    first_name: resp['fname'],
                    middle_name: '',
                    last_name: resp['lname'],
                    email: '',
                    mobile: resp['phoneNumber'],
                    products: registerData,
                    source:snapProducts[0]['source'],
                    _orig_referrer: c_snap_referrer_url,
                    _landing_page: c_snap_landing_page,
                    _shopify_sa_p: saPAttributes.value,
                    _shopify_sa_t: saTAttributes.value,
                    merchant_confirmation_url: snapOptions.merchant_confirmation_url,
                    merchant_failure_url: snapOptions.merchant_failure_url,
                };
                if (snapOptions.isCashbackCoupon && snapOptions.isCashbackCoupon === 'true' && resp['coupon']) {
                    data['coupon'] = resp['coupon'];
                }
                document.body.style.overflow = 'auto';
                if (snapOptions.autoMaticCoupon && snapOptions.autoMaticCoupon !== 'undefined') {
                    if (!data['coupon']) {
                        data['coupon'] = getAutoMaticCoupon(productdesc.snapOrderValue);
                    }
                }
                if (snapOptions.particularCoupon && snapOptions.particularCoupon !== 'undefined') {
                    data['coupon'] = eval(snapOptions.particularCoupon);
                }
                if (snapOptions.shippingFee && snapOptions.shippingFee !== 'undefined') {
                    let localQuantity = snapQuantity;
                    if (
                        window.location.href.includes(snapOptions.cartUrl) ||
                        getByIdOrClassName(snapOptions.miniCartDivId) ||
                        registerData.length > 0
                    ) {
                        localQuantity = 0;
                        for (let i = 0; i < registerData.length; i++) {
                            localQuantity += registerData[i].quantity;
                        }
                    }
                    let checkMinValue = '';
                    if (snapOptions.minForDelivery && snapOptions.minForDelivery !== 'undefined')
                        checkMinValue = snapOptions.minForDelivery;
                    if (
                        snapOptions.addingDeliveryMaxAmountExpress &&
                        snapOptions.addingDeliveryMaxAmountExpress !== 'undefined'
                    )
                        checkMinValue = snapOptions.addingDeliveryMaxAmountExpress;
                    if (parseInt(productdesc.snapOrderValue) < parseInt(checkMinValue)) {
                        data['shipping_id'] = snapOptions.shippingId;
                        if (snapOptions.shippingFeeUnit && snapOptions.shippingFeeUnit !== 'undefined') {
                            data['shipping_fee'] = snapOptions.shippingFeeUnit;
                        } else {
                            data['shipping_fee'] = parseInt(snapOptions.shippingFee) * localQuantity;
                        }
                        data['shipping_title'] = snapOptions.shippingTitle;
                    }
                }
                if (snapOptions.onWebsiteCoupon && snapOptions.onWebsiteCoupon !== 'undefined') {
                    if (onWebsiteCouponApplied) {
                        data['coupon'] = onWebsiteCouponApplied;
                    }
                }
                if(snapOptions.autoMaticCouponApply && snapOptions.autoMaticCouponApply !== 'undefined'){
                    if(!data['coupon']){
                        data['coupon'] = snapOptions.autoMaticCouponApply;
                    }
                }
                if (snapUnleashFlag) {
                    data['udf1'] = snapUnleashFlag;
                }
                let cartCreationResponse = await getToken(data); //cartId, cartToken
                if (cartCreationResponse.status.toLowerCase() === 'success') {
                    webEngageEvent('Mobile_Added_Snapmint', {
                        'mobile': resp['phoneNumber'],
                        'order_value': productdesc.snapOrderValue.toString(),
                        'first_name': resp['fname'],
                        'last_name': resp['lname'],
                        'promo_code': data['coupon'] ? data['coupon'] : '',
                        'currency': 'INR',
                    });
                    if(snapOptions.isGTMEnable && snapOptions.isGTMEnable !== 'undefined') {
                        dataLayer.push({
                            'event': 'cart_created',
                            values : {
                                'merchant_id': resp['merchant_id'], 
                            },
                            'page_location': window.location.href,
                            'page_path': window.location.pathname,
                            'merchant_name': snapOptions.merchantName,
                            'mobile': resp['phoneNumber'],
                            'order_value': productdesc.snapOrderValue.toString(),
                            'first_name': resp['fname'],
                            'last_name': resp['lname'],
                            'orig_referrer': c_snap_referrer_url,
                            'landing_page': c_snap_landing_page,
                            'shopify_sa_p': saPAttributes.value,
                            'shopify_sa_t': saTAttributes.value
                        });
                    }
                    let iframeUrl = cartCreationResponse.redirect_url + '&ecautosignIn=valid';

                    if (iframeUrl.includes('checkout.snapmint.com') || iframeUrl.includes('checkoutv2.snapmint.com') || iframeUrl.includes("pay.snapmint.com")) {
                        if (isSnapMerchantJSMobile) {
                            window.localStorage.setItem('autosnapminturl', iframeUrl);
                            document.getElementById('snap-modalon_page').style.display = 'none';
                            windowPopup = window.open(iframeUrl, 'mozillaWindow', windowFeaturesSnapmint);
                            checkpopup(windowPopup, iframeUrl);
                        } else {
                            window.localStorage.setItem('autosnapminturl', iframeUrl);
                            openCheckV2Iframe(iframeUrl, resp['fname'], resp['lname'], resp['phoneNumber']);
                        }
                    } else {
                        window.localStorage.setItem('autosnapminturl', iframeUrl);
                        document.getElementById('snap-modalon_page').style.display = 'none';
                        windowPopup = window.open(iframeUrl, 'mozillaWindow', windowFeaturesSnapmint);
                        checkpopup(windowPopup, iframeUrl);
                    }
                }
            }
            return 200;
        } else {
            var encrypted = CryptoJS.AES.encrypt(JSON.stringify(registerData), encryptionKeyCryptoJs);
            setDynamicJsCookie('snapmintDynamicJscookie', encrypted);
            return 400;
        }
    } catch (error) {
        return 400;
    }
}
const openCheckV2Iframe = (url, first_name, last_name, mobile_no) => {
    var checkOutV2 = document.createElement('iframe');
    checkOutV2.id = 'checkOutV2Frame';
    checkOutV2.setAttribute('src', url);
    // checkOutV2.setAttribute("src", "https://qacheckout.snapmint.com/?id=84906&message=Success&status=Success&token=nKSPTEpT9Bg9yjLTw-PJ")
    checkOutV2.setAttribute('allow', 'camera *; microphone *');
    // checkOutV2.setAttribute("src", "https://checkout.snapmint.com/?id=9690272&message=Success&status=Success&token=85eWvYV8xtXyBkE2zysz");
    checkOutV2.style.width = '100%';
    checkOutV2.style.height = '100%';
    checkOutV2.style.border = 'none';
    document.documentElement.style.overflow = 'hidden';
    if (document.querySelector('.snapLoader'))
        document.querySelector('.snapLoader').setAttribute('style', 'display:none;');
    if (document.querySelector('.coupoun_discount')) {
        if (document.querySelector('.modal-wrpr')) {
            document.querySelector('.modal-wrpr').style.display = 'none';
        }
    }
    if (document.querySelector('.snapmint_lowest_emi'))
        document.querySelector('.snapmint_lowest_emi').setAttribute('style', 'display:none;');
    if (document.querySelector('.snap_popup_bg'))
        document.querySelector('.snap_popup_bg').setAttribute('style', 'display:none;');
    if (document.querySelector('.snap_top_logo')) document.querySelector('.snap_top_logo').style.display = 'none';
    if (document.querySelector('.snap-close-wrpr')) document.querySelector('.snap-close-wrpr').style.display = 'none';
    if (document.getElementsByClassName('popuppiza') && document.getElementsByClassName('popuppiza')[0]) {
        document.getElementsByClassName('popuppiza')[0].style.border = 'none';
        document.getElementsByClassName('popuppiza')[0].style.width = 'fit-content';
        document.getElementsByClassName('popuppiza')[0].style.minWidth = '374px';
        document.getElementsByClassName('popuppiza')[0].style.maxWidth = '769px';
        document.getElementsByClassName('popuppiza')[0].style.height = '100vh';
        document.getElementsByClassName('popuppiza')[0].style.maxHeight = '600px';
        document.getElementsByClassName('popuppiza')[0].appendChild(checkOutV2);
    }
};
function addOutOfStockError() {
    if(isMiniCartPopupOpen) { return;}
    if (snapOptions.isVarientPopup && snapOptions.isVarientPopup === 'true') {return;}
    const isMiniCart = getByIdOrClassName(snapOptions.miniCartDivId);
     if (!isMiniCart) {
    if (document.querySelector('.snapLoader')) {
        document.querySelector('.snapLoader').setAttribute('style', 'display:none;');
    }
    if (document.querySelector('.snapmint_lowest_emi'))
        document.querySelector('.snapmint_lowest_emi').setAttribute('style', 'display:block;');
    var textChange = 'Select a size / color variant in stock & then continue with Snapmint EMI purchase';
    if (document.querySelector('.variant_validation')) {
        document.querySelector('.variant_validation').classList.add('active_vg');
        document.querySelector('.snap_how_works_ttl.snap_how_works_ttl-sml').textContent = textChange;
        document.querySelector('.snap_how_works_ttl.snap_how_works_ttl-sml').classList.add('var_size_style');
    } else {
        if (document.querySelector('.snap_variant_section')) {
            document.querySelector('.snap_variant_section').style.display = 'block';
            document.querySelector('.snap_how_works_section').classList.add('snap_varient_bg');
        }
    }
    if (snapOptions.isCashbackCoupon && snapOptions.isCashbackCoupon === 'true') {
        let element = document.querySelector('.snap_cupn_offers_btn');
        if (element) {
            document.querySelector('.snap_cupn_offers_btn').style.pointerEvents = 'none';
        }
    }
}
}
function checkpopup(windowPopup, iframeUrl) {
    if (windowPopup == null || typeof windowPopup == 'undefined') {
        windowPopup = window.open(iframeUrl, '_blank');
        if (windowPopup == null || typeof windowPopup == 'undefined') {
            if (document.querySelector('.snapmint_lowest_emi')) {
                document.querySelector('.snapmint_lowest_emi').style.display = 'none';
            }
            if (document.getElementById('snap-modalon_page')) {
                document.getElementById('snap-modalon_page').style.display = 'block';
            }
            if (document.querySelector('.snapLoader')) {
                document.querySelector('.snapLoader').setAttribute('style', 'display:block;');
            }
            if (document.querySelector('.rotating_img_unrotate')) {
                let img_rotate = document.querySelector('.rotating_img_unrotate');
                img_rotate.classList.remove('rotating');
                document.querySelector('.snap_processing_text').style.display = 'none';
            }
            if (document.querySelector('.snap_emi_purchase_btn')) {
                document.querySelector('.snap_emi_purchase_btn').addEventListener('click', function () {
                    if (document.querySelector('.popup_blocker')) {
                        document.querySelector('.popup_blocker').style.display = 'block';
                    }
                    if (document.querySelector('.snapLoader')) {
                        document.querySelector('.snapLoader').setAttribute('style', 'display:none;');
                    }
                });
            }
        }
    } else {
        if (iframeUrl.includes('checkout.snapmint.com') || iframeUrl.includes('checkoutv2.snapmint.com') || iframeUrl.includes("pay.snapmint.com")) {
            closeCheckV2Iframe();
        } else {
            closePopup();
        }
        window.localStorage.removeItem('autosnapminturl');
        windowPopup.focus();
    }
}

async function checkAuthenticated(cartId, token) {
    let authInterval = setInterval(async () => {
        let response = await checkotpAuthenticated(cartId, token);
        if (response.status === 'success') {
            clearInterval(authInterval);
            document.getElementById('snap-modalon_page').style.display = 'none';
            /* 25-01-2023 */
            document.body.style.overflow = 'auto';
            window.localStorage.removeItem('snapminturl');
            return false;
        }
    }, 7000);
}
function handleCloseButtonIframe() {
    if (document.getElementById('snapModalCloseon_iframe')) {
        document.getElementById('snapModalCloseon_iframe').addEventListener('click', function () {
            document.getElementsByTagName('html')[0].style.overflow = 'hidden';
            document.querySelector('.snapmint_lowest_emi_iframe').style.display = 'block';
            document.querySelector('.snapmint_lowest_emi_iframe .container').style.display = 'block';
        });
    }
}
function removeErrorMessages() {
    if (document.getElementById('err_fname')) {
        document.getElementById('err_fname').innerText = '';
        document.getElementById('fname').classList.remove('error_field');
    }
    if (document.getElementById('err_lname')) {
        document.getElementById('err_lname').innerText = '';
        document.getElementById('lname').classList.remove('error_field');
    }
    if (document.getElementById('err_num')) {
        document.getElementById('err_num').innerText = '';
        document.getElementById('mnumber').classList.remove('error_field');
    }
}
function closeCheckV2Iframe() {
    document.getElementById('snap-modalon_page').style.display = 'none';
    document.body.style.overflow = 'auto';
    closePopup();
}
Object.defineProperty(Array.prototype, 'clear', {
    value: function () {
        this.splice(0, this.length);
    }
});
function hostURL(version = 'v1') {
    return snapOptions.apiDomain + '/' + version;
}
function getToken(data) {
    const url = `${hostURL()}/shopify_plugin/get_token`;
    const requestOptions = {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(data)
    };
    return fetch(url, requestOptions)
        .then((res) => res.json())
        .then((res) => {
            return res;
        });
}
function changeExpressWidget(urlToShowWidgetExpress) {
    let urlArray = urlToShowWidgetExpress.split(',');
    let isShowWidget = false;
    if (urlArray && urlArray.length) {
        urlArray.forEach((element) => {
            if (window.location.href.includes(element)) {
                isShowWidget = true;
            }
        });
    }
    return isShowWidget;
}
if(snapOptions.clickOnAddToCartBtn && snapOptions.clickOnAddToCartBtn !== 'undefined'){
    window.addEventListener("load", (event) => {
        setTimeout(() => {
        var elementPdp = document.querySelector('#sm-widget-btn.snap_emi_txt');
        elementPdp.setAttribute("onclick","startPop();addToCartBtnClick();");
        }, 1000);
    });   
}
var variantarray = [];
function searchVariant(nameKey, myArray){
    for (let i=0; i < myArray.length; i++) {
        if (myArray[i].variant === nameKey) {
            return myArray[i];
        }
    }
}
function addToCartBtnClick() {

    const checkOutOfStock = document.querySelector('.snap_variant_section');
    if (checkOutOfStock && checkOutOfStock.style.display === 'block') return;
    var element = document.querySelector('#sm-widget-btn.snap_emi_txt');
    if (!element) return;
    var variationData = [];

    fetch(window.Shopify.routes.root + 'cart.js')
        .then((response) => response.json())
        .then((data) => {
            if (data.item_count > 0) {
                for (i = 0; i < data.items.length; i++) {
                    // products.clear();
                    var items = {

                        "variant": data.items[i].variant_id,
                    };
                    variationData.push(items);
                }

                const resultObject = searchVariant(parseInt(snapProducts[0].imei1), variationData);
                if (resultObject === undefined) {
                    let formData = {
                        items: [{
                            id: snapProducts[0].imei1,
                            quantity: snapQuantity
                        }]
                    };
                    fetch(window.Shopify.routes.root + 'cart/add.js', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify(formData)
                        })
                        .then((response) => {
                            window.dataLayer = window.dataLayer || [];
                            window.dataLayer.push({
                                'event': 'add_to_cart',
                                'items': [{
                                    'item_id': snapProducts[0].imei1,
                                    'name': snapProducts[0].name,
                                    'category': 'Apparel',
                                    'price': snapProducts[0].unit_price,
                                    'quantity': snapProducts[0].quantity
                                }]
                            });

                            return response.json();
                        })
                        .catch((error) => {
                            console.error('Error:', error);
                        });
                } 
            } else {
                let formData = {
                    items: [{
                        id: snapProducts[0].imei1,
                        quantity: snapQuantity
                    }]
                };
                fetch(window.Shopify.routes.root + 'cart/add.js', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify(formData)
                    })
                    .then((response) => {
                        window.dataLayer = window.dataLayer || [];
                        window.dataLayer.push({
                            'event': 'add_to_cart',
                              'currency': "INR",
                              'ecomm_prodid': snapProducts[0].imei1,
                              'ecomm_totalvalue':parseInt(snapProducts[0].unit_price),
                              'value':parseInt(snapProducts[0].unit_price),
                            'items': [{
                                'item_id': snapProducts[0].imei1,
                                'name': snapProducts[0].name,
                                'item_brand': snapOptions.merchantName,
                                'price': snapProducts[0].unit_price,
                                'quantity': snapProducts[0].quantity
                            }]
                        });

                        return response.json();
                    })
                    .catch((error) => {
                        console.error('Error:', error);
                    });
            }
        });
}function MiniCartOpen(secondCheckoutBtnId = '', isMultiple = false) {
    let add_Cart_btn = '';
    if (isMultiple) {
        add_Cart_btn = document.querySelectorAll(secondCheckoutBtnId);
        add_Cart_btn.forEach((element) => {
            addCartBtnEventListener(element);
        });
    } else {
        if (secondCheckoutBtnId) {
            add_Cart_btn = getByIdOrClassName(secondCheckoutBtnId);
            if (snapOptions.isMouseHoverSecondCheckout && snapOptions.isMouseHoverSecondCheckout !== 'undefined') {
                let throttle = false;
                add_Cart_btn.addEventListener('mouseover', () => {
                    if (throttle === false) {
                        throttle = true;
                        addinMiniCartOnHover();
                    }
                    let p = setTimeout(() => {
                        throttle = false;
                        clearTimeout(p);
                    }, 1000);
                });
            }
            if (snapOptions.thirdCheckoutBtnId && snapOptions.thirdCheckoutBtnId !== 'undefined') {
                let eventType = 'click';
                if (snapOptions.eventType && snapOptions.eventType !== 'undefined') {
                    eventType = snapOptions.eventType;
                }
                add_Cart_btn.addEventListener(eventType, () => {
                    addinMiniCartOnHover();
                });
            }
        } else {
            add_Cart_btn = getByIdOrClassName(snapOptions.checkoutBtnId);
        }
        addCartBtnEventListener(add_Cart_btn);
    }
    function addCartBtnEventListener(add_Cart_btn) {
        if (add_Cart_btn) {
            let eventType = 'click';
            if (snapOptions.miniCartEventType && snapOptions.miniCartEventType !== 'undefined') {
                eventType = snapOptions.miniCartEventType;
            }
            add_Cart_btn.addEventListener(eventType, () => {
                let counter = 1;
                let id = setInterval(() => {
                    counter++;
                    let mini_cart = getByIdOrClassName('.cart-drawer.active .checkout');
                    if (mini_cart) {
                        if(snapOptions.isSubtotalVisible && snapOptions.isSubtotalVisible !== 'undefined')
                        fetchDataForShopify();
