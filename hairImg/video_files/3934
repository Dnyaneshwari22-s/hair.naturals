var snapMintAssetUrl = 'https://assets.snapmint.com/';
var encryptionKeyCryptoJs = 'Secret Passphrase';
var snapMintEvent = null;
var snapUnleashFlag = '';
var onWebsiteCouponApplied = null;
var snapMintCouponList = [];
var webEngageInfo = [];
var isMiniCartPopupOpen = false;
if (snapOptions.stopScroll === 'undefined') {
    snapOptions.stopScroll = 'true';
}
if(snapOptions.scrollProperty === 'undefined') {
    snapOptions.scrollProperty = 'auto';
}
const isSnapMerchantJSMobile =
    /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ||
    /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.platform);
const getByID = (id) => document.getElementById(id);


let downPayment = 0;
let emi = 0;
let snapOrderValue = 0;
let snapQuantity = 1;
let discountedOrderValue = 0;
let snapDiscountCode = '';
let orderValueNotChange = 0;
let miniCartQuantity = 1;
let showWidgetExpress = false;
async function loadOnPage() {
        if (snapOptions.checkDomain && snapOptions.checkDomain !== 'undefined')

    {
        if((window.location.href.indexOf(snapOptions.domain) != -1))
        {
        }else
        {
            return;
        }
    }
    let priceTag = '';
    if (getByIdOrClassName(snapOptions.priceSaleDivId)) {
        priceTag = getByIdOrClassName(snapOptions.priceSaleDivId);
    } else {
        priceTag = getByIdOrClassName(snapOptions.priceDivId);
    }
    if (
        snapOptions.findAmount &&
        snapOptions.findAmount !== 'undefined' &&
        !window.location.href.includes(snapOptions.cartUrl)
    ) {
        snapOrderValue = parseNum(eval(snapOptions.findAmount)) * snapQuantity;
    } else {
         if(priceTag){
                   snapOrderValue = parseNum(priceTag.innerText) * snapQuantity;
                }else
                {
                    snapOrderValue=productdesc.snapOrderValue;
                }
    }
    if (!priceTag) {
        throw new Error('EMPTY_ORDER_VALUE');
    }
    if (findPopup() === 0) {
        let element = getByIdOrClassName('#sm-widget-btn');
        if (element) element.parentNode.removeChild(element);
        if (snapOptions.isTwoWidget === 'true') {
            let newElement = getByIdOrClassName('.second_widget_btn');
            if (newElement) newElement.parentNode.removeChild(newElement);
        }
        if (snapOptions.errorWidgetPage && snapOptions.errorWidgetPage !== 'undefined') addingErrorWidget(snapOrderValue);
        return;
    } else {
        if (snapOptions.errorWidgetPage && snapOptions.errorWidgetPage !== 'undefined') removingErrorWidget();
    }

        if (priceTag) {
        const minOrderVal = parseNum(snapOptions.plans[0].minAmount);
        let parentDiv = getByIdOrClassName(snapOptions.divId);
        if (checkPdpIsMobile()) parentDiv = getByIdOrClassName(snapOptions.mobileDivId);
        if (minOrderVal > snapOrderValue) {
            let errorWidget = snapOptions.errorWidgetPage;
            getByID('dp').textContent = downPayment;
            parentDiv.innerHTML = errorWidget;
            return;
        }
        if(snapOptions.discountPercent && snapOptions.discountPercent!== 'undefined' && snapOptions.showDiscountPercentAll && snapOptions.showDiscountPercentAll!== 'undefined' ){
        snapOrderValue=Math.round(snapOrderValue-(snapOrderValue*snapOptions.discountPercent));
        }

        for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];

            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                downPayment = Math.round((snapOrderValue * plan.dpPercent) / 100);
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
            }
        }

        if (!downPayment) {
            downPayment = snapOrderValue / 10;
        }

        if (!emi) {
            emi = parseFloat(snapOrderValue * 10) / 100.0;
        }
        let widgetPage = snapOptions.widgetPriceBreakdown && snapOptions.widgetPriceBreakdown !== "undefined" ?  snapGetWidgetHtml(snapOrderValue) : snapOptions.widgetPage;
        if (
            !window.location.href.includes(snapOptions.cartUrl) &&
            snapOptions.pdpPayLaterAmount &&
            snapOptions.pdpPayLaterAmount !== 'undefined'
        ) {
            if (snapOrderValue < parseInt(snapOptions.pdpPayLaterAmount)) {
                widgetPage = getPayLaterWidget(snapOptions.pdpPayLaterAmount, snapOptions.widgetPage, 'pdp');
            } else {
                let element = getByIdOrClassName('#sm-widget-btn');
                if (element) element.parentNode.removeChild(element);
                if (snapOptions.isTwoWidget === 'true') {
                    let newElement = getByIdOrClassName('.second_widget_btn');
                    if (newElement) newElement.parentNode.removeChild(newElement);
                }
                widgetPage = snapOptions.widgetPage;
            }    
        } else if (
            window.location.href.includes(snapOptions.cartUrl) &&
            snapOptions.cartPayLaterAmount &&
            snapOptions.cartPayLaterAmount !== 'undefined'
        ) {
            widgetPage = getPayLaterWidget(snapOptions.cartPayLaterAmount, snapOptions.widgetPage, 'cart');
        }
        if(snapOptions.changeWidget && snapOptions.changeWidget !== 'undefined' && snapOptions.changeWidget !== null){
            eval(snapOptions.changeWidget);
        }
        if (snapOptions.urlToShowWidgetExpress && snapOptions.urlToShowWidgetExpress !== 'undefined') {
            if (!window.location.href.includes(snapOptions.cartUrl)) {
                if (changeExpressWidget(snapOptions.urlToShowWidgetExpress)) {
                    widgetPage = snapOptions.widgetExpressUrl;
                    showWidgetExpress = true;
                }
            }
        }
        if (snapOptions.cartWidgetExpressUrl && snapOptions.cartWidgetExpressUrl !== 'undefined') {
            if (window.location.href.includes(snapOptions.cartUrl)) {
                if (checkWhichWidgetToLoad(snapOptions.cartWidgetExpressUrl)) {
                    widgetPage = snapOptions.checkForCartWidget;
                    showWidgetExpress = true;
                } else {
                    showWidgetExpress = false;
                }
            }
        }

       /**  Below if condition will check if there is multiple product on pdp page and if true then it will show widget on all products based on their pricing */
       
        if (snapOptions.isPDPMultiWidget && snapOptions.isPDPMultiWidget !== undefined) {
            eval(snapOptions.executePDPMultiWidget);
        }

        if (!getByIdOrClassName('#sm-widget-btn')) {
            if (
                snapOptions.unleashAbTest &&
                snapOptions.unleashAbTest !== 'undefined' &&
                !getByIdOrClassName(snapOptions.miniCartDivId) &&
                !window.location.href.includes(snapOptions.cartUrl)
            ) {
                widgetPage = widgetPage.replace(
                    '<div id="sm-widget-btn"',
                    '<div id="sm-widget-btn" style="visibility:hidden !important"'
                );
            } else {
                if (snapOptions.position === 'before') parentDiv.insertAdjacentHTML('beforebegin', widgetPage);
                else parentDiv.insertAdjacentHTML('afterend', widgetPage);

                addingCssStyle(snapOptions.style, '#sm-widget-btn');
                widgetPage = widgetPage.replace(
                    '<div id="sm-widget-btn"',
                    '<div id="sm-widget-btn" style="visibility:visible !important"'
                );
            }
            //adding posthog event for pdp widget
            if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
                // if (!window.location.href.includes(snapOptions.cartUrl)) {
                addPosthogEvent();
                // }
            }
        }
        if (
            snapOptions.unleashAbTest &&
            snapOptions.unleashAbTest !== 'undefined' &&
            !getByIdOrClassName(snapOptions.miniCartDivId) &&
            !window.location.href.includes(snapOptions.cartUrl)
        ) {
            await snapAddUnleashScript();
        }
        // after  the widget is added to DOM, for checking isTwoWidget, pdpCashbackText, cartCashbackText, merchantLogo, addingDynamicPlan, isAbTest, cartWidgetExpressUrl, isCashbackCoupon & pdpPayLaterAmount
        loadDataAfterWidgetLoad();
    }
}
function loadDataAfterWidgetLoad() {
          if((snapOptions.showPaywidgetEmi== 'true') || (snapOptions.miniCartshowPaywidgetEmii== 'true')){
    if (getByID('dp')) getByID('dp').textContent = emi;
}else
{
     if (getByID('dp')) getByID('dp').textContent = downPayment;
}
      let elementpdp = getByIdOrClassName('#sm-widget-btn');
        elementpdp.setAttribute("data-widget",snapOrderValue);

    if (snapOptions.isTwoWidget === 'true' && !window.location.href.includes(snapOptions.cartUrl)) {
        loadSecondWidget();
    }
    if (
        (snapOptions.pdpCashbackText && snapOptions.pdpCashbackText !== 'undefined') ||
        (snapOptions.cartCashbackText && snapOptions.cartCashbackText !== 'undefined')
    ) {
        updateCashbackWidget(
            window.location.href.includes(snapOptions.cartUrl)
                ? snapOptions.cartCashbackText
                : snapOptions.pdpCashbackText
        );
    }
    if (snapOptions.merchantLogo && snapOptions.merchantLogo !== 'undefined') {
        whiteLabelChange();
    }
    if (snapOptions.addingDynamicPlan && snapOptions.addingDynamicPlan !== 'undefined') {
        addDynamicPlan();
    }
    if (snapOptions.isAbTest && snapOptions.isAbTest !== 'undefined') {
        addDynamicData();
    }
    if (snapOptions.pdpPayLaterAmount && snapOptions.pdpPayLaterAmount !== 'undefined') {
        addDynamicPlanWidget();
    }
    if (snapOptions.isCashbackCoupon && snapOptions.isCashbackCoupon !== 'undefined') {
        if (!window.location.href.includes(snapOptions.cartUrl)) {
            if (!getDynamicJsCookie('snapmintDynamicJscookie')) {
                addClassForAutoMaticCoupon(snapOrderValue);
            }
        }
        if (
            snapOptions.isTest &&
            snapOptions.isTest !== 'undefined' &&
            window.location.href.includes(snapOptions.cartUrl)
        ) {
            if (!getDynamicJsCookie('snapmintDynamicJscookie')) {
                addClassForAutoMaticCoupon(snapOrderValue);
            }
        }
    }
    if (snapOptions.cartWidgetExpressUrl && snapOptions.cartWidgetExpressUrl !== 'undefined') {
        if (window.location.href.includes(snapOptions.cartUrl)) {
            if (showWidgetExpress === false) {
                let element = document.querySelector('#sm-widget-btn.snap_emi_txt');
                if (element) {
                    element.style.border = 'none';
                    element.classList.add('widget_express_on_url');
                }
            }
        }
    }

    if (snapOptions.urlToShowWidgetExpress && snapOptions.urlToShowWidgetExpress !== 'undefined') {
        if (!window.location.href.includes(snapOptions.cartUrl)) {
            if (showWidgetExpress === false) {
                let element = document.querySelector('#sm-widget-btn.snap_emi_txt');
                if (element) {
                    element.style.border = 'none';
                    element.classList.add('widget_express_on_url');
                }
            }
        }
        if (snapOptions.isPdpQuickView && snapOptions.isPdpQuickView !== 'undefined') {
            MiniCartOpen();
        }
    }
    if (snapOptions.personaliseDivId && snapOptions.personaliseDivId !== 'undefined') {
        const radioButtons = document.querySelectorAll(snapOptions.personaliseDivId);
        if (radioButtons && radioButtons.length > 0) {
            radioButtons.forEach((button) => {
                if (button.checked) {
                    if (button.value === 'Yes') {
                        let element = getByIdOrClassName('.snap_emi_txt.snap_emi_txt_wrapper');
                        let element_2_0 = getByIdOrClassName('.snap_emi_txt_wrapper.snap_emi_txt.second_widget_btn');
                        if (element) {
                            element.style.display = 'none';
                            element.style.pointerEvents = 'none';
                        }
                        if (element_2_0) {
                            element_2_0.style.display = 'none';
                            element_2_0.style.pointerEvents = 'none';
                        }
                    }
                }
            });
        }
        if (radioButtons && radioButtons.length > 0) {
            let element = getByIdOrClassName('.snap_emi_txt.snap_emi_txt_wrapper');
            let element_2_0 = getByIdOrClassName('.snap_emi_txt_wrapper.snap_emi_txt.second_widget_btn');
            radioButtons.forEach((button) => {
                button.addEventListener('change', function () {
                    
                    if ((this.value === 'Yes' &&  this.type === 'radio' ) || (this.checked == true && this.type === 'checkbox' ) ){
                        
                        if (element) {
                            element.style.display = 'none';
                            element.style.pointerEvents = 'none';
                        }
                        if (element_2_0) {
                            element_2_0.style.display = 'none';
                            element_2_0.style.pointerEvents = 'none';
                        }
                    } else if ((this.value === 'No' &&  this.type === 'radio' ) || ( this.checked === false && this.type === 'checkbox')) {
                        
                        if (element) {
                            element.style.display = 'block';
                            element.style.pointerEvents = 'auto';
                        }
                        if (element_2_0) {
                            element_2_0.style.display = 'block';
                            element_2_0.style.pointerEvents = 'auto';
                        }
                    }
                
                });
            });
        }
    }
}
function addDynamicPlanWidget() {
    let allDate = document.querySelectorAll('.snap_emi_date_widget');
    if (allDate.length > 0) {
        allDate.forEach((element) => {
            if (snapOptions.fullMonthName && snapOptions.fullMonthName !== 'undefined') {
                element.innerHTML = addMonths(new Date(), 1, true);
            } else {
                element.innerHTML = addMonths(new Date(), 1);
            }
        });
    }
}
function getPayLaterWidget(amount, widgetType, pageType) {
    if (pageType === 'miniCart') {
        if (getByIdOrClassName('.snap_emi_txt_minicart')) {
            let id = getByIdOrClassName('.snap_emi_txt_minicart');
            id.parentNode.removeChild(id);
        }
    } else {
        if (getByIdOrClassName('#sm-widget-btn')) {
            if (snapOptions.pdpPayLaterSecondWidget && snapOptions.pdpPayLaterSecondWidget !== 'undefined') {
                let allElement = document.querySelectorAll('#sm-widget-btn');
                if (allElement.length > 0) {
                    allElement.forEach((element) => {
                        element.parentNode.removeChild(element);
                    });
                }
            } else {
                let id = getByIdOrClassName('#sm-widget-btn');
                id.parentNode.removeChild(id);
            }
        }
    }
    if (snapOptions.payLaterWidgetAmountCheck && snapOptions.payLaterWidgetAmountCheck !== 'undefined') {
        if(pageType === 'miniCart'){
            if(snapOptions.isSubtotalVisible && snapOptions.isSubtotalVisible !== 'undefined'){
                if(parseInt(snapOptions.isSubtotalVisible) > parseInt(snapOptions.pdpPayLaterAmount)){
                    return widgetType;
                }
            }
        }
        if (parseInt(snapOrderValue) > parseInt(snapOptions.pdpPayLaterAmount)) {
            return widgetType;
        }
    }
    if (amount && snapOptions.plans && snapOptions.plans.length > 0) {
        if (parseInt(snapOrderValue) < parseInt(snapOptions.plans[0].maxAmount)) {
            if (pageType === 'pdp') {
                return snapOptions.pdpPayLaterWidget;
            } else if (pageType === 'cart') {
                return snapOptions.cartPayLaterWidget;
            } else if (pageType === 'miniCart') {
                return snapOptions.miniCartPayLaterWidget;
            }
        } else {
            return widgetType;
        }
    }
    return widgetType;
}
function addDynamicData() {
    let element = getByIdOrClassName('#sm-widget-btn');
    if (element) {
        var queryString = window.location.search;
        var urlParams = new URLSearchParams(queryString);
        var variant = urlParams.get('variant');
        element.setAttribute('data-ga-view-custom', '');
        element.setAttribute('data-ga-view-info-custom', 'product_emi_option');
        element.setAttribute('data-ga-view-payload-custom', '{product_id:' + variant + '}');
    }
}

function getPlan(plan) {
    if (plan < 1 || plan > snapOptions.plans.length) {
        return;
    }
    const tenures = [];
    for (let i = 0; i < plan; i++) {
        tenures.push(snapOptions.plans[i].tenure);
    }
    return tenures.join('/');
}
function addDynamicPlan() {
    const element = document.querySelectorAll('.dynamic_emis');
    if (element && element.length > 0) {

var emitext=[];
for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];

            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                downPayment = Math.round((snapOrderValue * plan.dpPercent) / 100);
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
                emitext.push(plan.tenure);
            }
        }

        var dynam_plna=emitext.toString();
        dynam_plna=dynam_plna.replace(/,/g, "/"); 

             element.forEach((element)=>{
            element.innerHTML = dynam_plna;
        }   );
    }
}   
function addDynamicPlanMiniCart() {
    const element = document.querySelectorAll('.dynamic_emis_minicart');
    if (element && element.length > 0) {
    var emitext=[];
    for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];

            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                downPayment = Math.round((snapOrderValue * plan.dpPercent) / 100);
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
                emitext.push(plan.tenure);
            }
        }

        var dynam_plna=emitext.toString();
        dynam_plna=dynam_plna.replace(/,/g, "/"); 

             element.forEach((element)=>{
            element.innerHTML = dynam_plna;
        }   );
    }
}
function addingErrorWidget(snapOrderValue) {
    const minOrderVal = parseNum(snapOptions.plans[0].minAmount);
    if (minOrderVal > snapOrderValue) {
        let parentDiv = getByIdOrClassName(snapOptions.divId);
        if (checkPdpIsMobile()) parentDiv = getByIdOrClassName(snapOptions.mobileDivId);
        let widgetPage = snapOptions.errorWidgetPage;
        if (!getByIdOrClassName('#sm-widget-btn .error_widget')) {
            if (snapOptions.position === 'before') parentDiv.insertAdjacentHTML('beforebegin', widgetPage);
            else parentDiv.insertAdjacentHTML('afterend', widgetPage);
            addingCssStyle(snapOptions.style, '#sm-widget-btn');
        }
        getByID('dp').textContent = minOrderVal;
    }
}

function updateCashbackWidget(text) {
    if (!text) return;
    let element = document.querySelector('.cashback_text_widget');
    if (element) {
        element.style.display = 'block';
        let enterText = document.querySelector('.cashback_text_enter');
        if (enterText) {
            enterText.innerHTML = text;
        }
    }
}

function whiteLabelChange() {
    if (snapOptions.merchantLogoWidget && snapOptions.merchantLogoWidget !== 'undefined') {
        let all_element = document.querySelectorAll('.snap_merchant_logo_add_widget');
        if (all_element.length > 0) {
            all_element.forEach((element) => {
                element.src = snapOptions.merchantLogoWidget;
            });
        }
    }
    if (snapOptions.merchantName && snapOptions.merchantName !== 'undefined') {
        let all_element = document.querySelectorAll('.white_label_comany_name');
        if (all_element.length > 0) {
            all_element.forEach((element) => {
                element.textContent = snapOptions.merchantName;
            });
        }
    }
    if (snapOptions.merchantLogo && snapOptions.merchantLogo !== 'undefined') {
        let all_element = document.querySelectorAll('.snap_merchant_logo_add');
        if (all_element.length > 0) {
            all_element.forEach((element) => {
                element.src = snapOptions.merchantLogo;
            });
        }
    }
    if (snapOptions.merchantSlogan && snapOptions.merchantSlogan !== 'undefined') {
        let all_element = document.querySelectorAll('.snap_emi_slogan_text');
        if (all_element.length > 0) {
            all_element.forEach((element) => {
                element.innerHTML = snapOptions.merchantSlogan;
            });
        }
    }
    if (snapOptions.white_label_month_text && snapOptions.white_label_month_text !== 'undefined') {
        let all_element = document.querySelectorAll('.snap_emi_white_month_text');
        if (all_element.length > 0) {
            all_element.forEach((element) => {
                element.innerHTML = snapOptions.white_label_month_text;
            });
        }
    }
    if (snapOptions.white_label_text && snapOptions.white_label_text !== 'undefined') {
        let all_element = document.querySelectorAll('.snap_emi_white_text');
        if (all_element.length > 0) {
            all_element.forEach((element) => {
                element.innerHTML = snapOptions.white_label_text;
            });
        }
    }
}

function updateCashbackWidgetMiniCart(text) {
    if (!text) return;
    let element = document.querySelector('.cashback_text_widget_minicart');
    if (element) {
        element.style.display = 'block';
        let enterText = document.querySelector('.cashback_text_enter_minicart');
        if (enterText) {
            enterText.innerHTML = text;
        }
    }
}

function removingErrorWidget() {
    if (getByIdOrClassName('.error_widget')) {
        let element = getByIdOrClassName('.error_widget');
        element.parentNode.removeChild(element);
    }
}

function addingCssStyle(style, tag, number = 0) {
    var styleElement = document.createElement('style');
    styleElement.innerHTML = style;
    if (styleElement.styleSheet) {
        styleElement.styleSheet.cssText = style;
    } else styleElement.appendChild(document.createTextNode(style));
    if (number === 0) {
        getByIdOrClassName(tag).appendChild(styleElement);
    } else {
        let element = document.querySelectorAll(tag);
        if (element.length > 1) {
            element[1].appendChild(styleElement);
            element[1].classList.add('second_widget_btn');
        }
    }
}

function checkPdpIsMobile() {
    if (window.innerWidth < 768) {
        if (snapOptions.mobileDivId && snapOptions.mobileDivId !== 'undefined') {
            return true;
        }
    }
    return false;
}

function getSecondWidgetUsingPayLater(amount, widgetType, pageType) {
    if (amount && snapOptions.plans && snapOptions.plans.length > 0) {
        if (parseInt(snapOrderValue) < parseInt(snapOptions.plans[0].maxAmount)) {
            if (pageType === 'pdp') {
                return snapOptions.pdpPayLaterSecondWidget;
            }
        } else {
            return widgetType;
        }
    }
    return widgetType;
}
function loadSecondWidget() {
    let widgetPage = snapOptions.secondWidgetPage;
    if (snapOptions.pdpPayLaterSecondWidget && snapOptions.pdpPayLaterSecondWidget !== 'undefined') {
        widgetPage = getSecondWidgetUsingPayLater(snapOptions.pdpPayLaterAmount, snapOptions.secondWidgetPage, 'pdp');
    }
    let parentDiv = getByIdOrClassName(snapOptions.secondDivId);
    if (document.querySelectorAll('#sm-widget-btn').length === 1) {
        if (snapOptions.secondPosition === 'before') parentDiv.insertAdjacentHTML('beforebegin', widgetPage);
        else parentDiv.insertAdjacentHTML('afterend', widgetPage);
        addingCssStyle(snapOptions.secondStyle, '#sm-widget-btn', 1);
    }
    let allDpClass = document.querySelectorAll('.dp-class');
    if (allDpClass.length > 0) {
        allDpClass.forEach((element) => {
           if((snapOptions.showPaywidgetEmi== 'true') || (snapOptions.miniCartshowPaywidgetEmii== 'true')){
                element.textContent = emi;
            }else
            element.textContent = downPayment;
        });
    }
}
function closePopup() {
    const targetDiv = document.getElementById('sm-pop-up-model');
    if (targetDiv) {
        document.body.removeChild(targetDiv);
        document.documentElement.style.overflow = snapOptions.scrollProperty;
    }
    if (snapOptions.isTest && snapOptions.isTest === 'true') {
        if (!getByIdOrClassName(snapOptions.miniCartDivId)) {
            loadOnPage();
        }
    }
    if (snapOptions.stopScroll && snapOptions.stopScroll === 'true') {
        document.body.style.height = '';
        document.body.style.overflow = snapOptions.scrollProperty;
    }

    if (snapOptions.modalInputIsNotClickable && snapOptions.modalInputIsNotClickable !== 'undefined') {
        let element = document.querySelector(snapOptions.modalInputIsNotClickable);
        if (element) {
            element.style.visibility = 'visible';
        }
     } 
}

function snapclosePopup() {
    const targetDiv = document.getElementById('sm-pop-up-model');
    if (targetDiv) {
        document.body.removeChild(targetDiv);
        document.documentElement.style.overflow = snapOptions.scrollProperty;
    }
    if (snapOptions.stopScroll && snapOptions.stopScroll === 'true') {
        document.body.style.height = '';
        document.body.style.overflow = snapOptions.scrollProperty;
    }
    if (snapOptions.modalInputIsNotClickable && snapOptions.modalInputIsNotClickable !== 'undefined') {
        let element = document.querySelector(snapOptions.modalInputIsNotClickable);
        if (element) {
            element.style.visibility = 'visible';
        }
    }
}

function snapclosePopupss() {
    const targetDiv = document.getElementById('sm-pop-up-model');
    if (targetDiv) {
        document.body.removeChild(targetDiv);
    }
    if (snapOptions.modalInputIsNotClickable && snapOptions.modalInputIsNotClickable !== 'undefined') {
        let element = document.querySelector(snapOptions.modalInputIsNotClickable);
        if (element) {
            element.style.visibility = 'visible';
        }
    }
}

function replaceAllValuesInsideClass(tagName, assignValue) {
    const elements = document.getElementsByClassName(tagName);
    for (let i = 0; i < elements.length; i++) {
        elements[i].innerText = assignValue;
    }
}

function replaceValuesForNonExpress(downPayment, emi) {
    let allDownPayment = document.querySelectorAll('.downPayment');
    if (allDownPayment.length > 0) {
        allDownPayment.forEach((element) => {
            element.innerText = downPayment;
        });
    }
}

function secondWidgetClick(element) {
    if (snapOptions.pdpPayLaterSecondWidget && snapOptions.pdpPayLaterSecondWidget !== 'undefined') {
        if (element) {
            let classList = element.classList;
            if (classList.contains('second_widget_btn')) return true;
        }
    } else {
        if (element) {
            let classList = element.classList;
            if (classList.contains('second_widget_btn')) return true;
            else return false;
        }
    }
    let parentNodeHave = false;
    let parentNode = event.target.parentNode;
    if (parentNode) parentNode = parentNode.classList;
    if (parentNode && parentNode.contains('second_widget_btn')) {
        parentNodeHave = true;
    }
    if (!parentNodeHave) {
          if (!parentNode) 
       {
        parentNode=document.querySelector("#sm-widget-btn");
         parentNodeHave = true;
       }
        if (parentNode) parentNode = parentNode.classList;
        if (parentNode && parentNode.contains('second_widget_btn')) {
            parentNodeHave = true;
        }
        try {
            if (!parentNodeHave) {
                parentNode = event.target.parentNode.parentNode.parentNode;
                if (parentNode) parentNode = parentNode.classList;
                if (parentNode && parentNode.contains('second_widget_btn')) {
                    parentNodeHave = true;
                }
            }
        } catch (e) {}
    }
    return parentNodeHave;
}
async function getVationOnPopup() {
     sessionStorage.removeItem("sizeslected");
    if (document.querySelector(snapOptions.checkDisableCheckout)) {
        document.querySelector(".snap_category_dropdown").style.display = "none";
        document.querySelector(".snap_category_form").style.display = "block";
        document.querySelector(".snap_info_form").style.display = "none";
        document.querySelector(".sizeError").style.display = "none";
        //    document.querySelector(".snap_pd_added .snap_category_label.snap_form_label").style.display = "none";
    } else {
        document.querySelector(".snap_category_dropdown").style.display = "none";
        document.querySelector(".snap_category_form").style.display = "none";
        document.querySelector(".snap_info_form").style.display = "block";
        document.querySelector(".sizeError").style.display = "none";
        var e = document.querySelectorAll(snapOptions.selectedSize);
        var multisize=[];
        e.forEach(function(b) {
                   multisize.push( b.innerText);
        })
        multisize= multisize.toString();
        multisize=multisize.replace(/[,]/g, '/');
        document.querySelectorAll(".snap_selected_item .snap_item_bold")[1].innerHTML = multisize;
        document.querySelector("div.snap_ddd").innerText = multisize;
        document.querySelector(".snap_category_form .snap_category_label.snap_form_label").style.removeProperty("display");
        document.querySelector(".snap_category_form .snap_input_section").classList.add("snap_pd_added");
    }
    if (document.querySelector(snapOptions.selectedColor)) {
        var colorOption = document.querySelector(snapOptions.selectedColor).innerText;
        document.querySelectorAll(".snap_category_form .snap_selected_item .snap_item_bold")[0].innerHTML = colorOption;
        document.querySelectorAll(".snap_selected_item .snap_item_bold")[0].innerHTML = colorOption;
    }
    var variationOptions = [];
    var variationOptionsStock = [];
    var newURL = window.location.href;
    var splitURL = newURL.toString().split("?");

    try {
        const response = await fetch(splitURL['0'] + ".js");
        if (!response.ok) {
            throw new Error(`Response status: ${response.status}`);
        }

        const json = await response.json();
        var Snap_variants = json.variants;
        for (i = 0; i < Snap_variants.length; i++) {

            if (Snap_variants[i]['id'] == snapProducts[0]['imei1']) {

                if (Snap_variants[i]['available'] === false) {

                    document.querySelector('.snap_cupn_offers_btn').style.pointerEvents = 'none';
                    document.querySelector('.snap_variant_section').style.display = 'block';
                    document.querySelector('.snap_how_works_section').classList.add('snap_varient_bg');
                } else {
                    document.querySelector('.snap_cupn_offers_btn').style.pointerEvents = 'auto';
                    document.querySelector('.snap_variant_section').style.display = 'none';
                    document.querySelector('.snap_how_works_section').classList.remove('snap_varient_bg');
                }
            }
            if (Snap_variants[i]['available'] === true) {


                var htmlOptions = '<div class="snap_category_options_text" value="' + Snap_variants[i]['id'] + '">' + Snap_variants[i]['title'] + '</div>';
                variationOptions.push(htmlOptions);

            } else {

                var htmlOptions = '<div class="snap_category_options_text snap_out_of_stock_option" value="' + Snap_variants[i]['id'] + '"><span>' + Snap_variants[i]['title'] + '</span> <span>Out of Stock</span></div>';
                variationOptions.push(htmlOptions);
            }

        }

        variationOptions = variationOptions.toString();
        document.querySelector(".snap_category_option").innerHTML = variationOptions.replace(/[,]/g, '');

    } catch (error) {
        console.error(error.message);
    }

    var snap_category = document.querySelector('.snap_category_form_btn');
    if (snap_category) {
        snap_category.addEventListener('click', function(event) {
            if (!document.querySelector('.snap_category_form .snap_input_section.snap_first_input').classList.contains('snap_pd_added')) {
                document.querySelector(".sizeError").style.display = "block";
            } else {
                document.querySelector(".snap_category_dropdown").style.display = "none";
                document.querySelector(".snap_category_form").style.display = "none";
                document.querySelector(".sizeError").style.display = "none";
                document.querySelector(".snap_info_form").style.display = "block";
                if (document.querySelector(snapOptions.selectedColor)) {
                    var colorOption = document.querySelector(snapOptions.selectedColor).innerText;
                    document.querySelectorAll(".snap_selected_item .snap_item_bold")[0].innerHTML = colorOption;
                }
            }

        });
    }

    var snap_category = document.querySelector('.snap_edit_btn');
    if (snap_category) {
        snap_category.addEventListener('click', function(event) {
            document.querySelector(".snap_category_form").style.display = "block";
            document.querySelector(".snap_info_form").style.display = "none";
        });
    }
    var snap_field_arrow = document.querySelector('.snap_category_form .snap_input_section.snap_first_input');
    if (snap_field_arrow) {
        snap_field_arrow.addEventListener('click', function(event) {


            document.querySelector(".snap_category_dropdown").style.display = "block";

            var snap_category_option_click = document.querySelectorAll('.snap_category_options_text');
            if (snap_category_option_click) {

                snap_category_option_click.forEach(function(b) {

                    b.addEventListener('click', function(event) {

                        document.querySelector(".snap_category_form .snap_category_dropdown").style.display = "none";

                        document.querySelectorAll('.snap_category_options_text').forEach(d => d.removeAttribute("id"));

                        var vatvalue = event.target.getAttribute("value");
                        event.target.setAttribute("id", "snap_selected_category_option");
                        document.querySelectorAll(".snap_selected_item .snap_item_bold")[1].innerHTML = event.target.innerText;
                        document.querySelector("div.snap_ddd").innerText = event.target.innerText;
                        snapProducts[0]['imei1'] = vatvalue;
                        document.querySelector(".sizeError").style.display = "none";
                         sessionStorage.setItem("sizeslected", "true");
                        document.querySelector(".snap_category_form .snap_category_label.snap_form_label").style.removeProperty("display");
                        document.querySelector(".snap_category_form .snap_input_section").classList.add("snap_pd_added");
                        setTimeout(() => {
                             var x = getDynamicJsCookie('snapmintDynamicJscookie');
                            if(b.hasAttribute('id') && x && sessionStorage.getItem('sizeslected')=="true")
                            {
                                 handleServiceWorker(snapProducts, productdesc, 'checkcokiee');
                            }
                            document.querySelector(".snap_category_form .snap_category_dropdown").style.display = "none";
                        }, 100);
                    });
                });
            }
            var snap_pop = document.querySelectorAll('.snap-installment,.snap_middle_section');
            if (snap_pop) {
                snap_pop.forEach(function(b) {
                    b.addEventListener('click', function(event) {
                        document.querySelector(".snap_category_dropdown").style.display = "none";
                    });

                });
            }

        });
    }
}

function startPop(element) {
    snapMintEvent = element;
    if (snapOptions.categoryWidgetHtml && snapOptions.categoryWidgetHtml !== 'undefined') {
        if (element && element.querySelector('.dp-collection-price')) {
            let getCurrentPrice = element.querySelector('.dp-collection-price').getAttribute('data-price');
            if (getCurrentPrice) {
                snapOrderValue = parseNum(getCurrentPrice);
                getDownPaymentAndEmi(snapOrderValue);
            }
        }
    }
    if(snapOptions.isWebEngageEnable && snapOptions.isWebEngageEnable !== 'undefined'){
        webEngageInitiate();
    }
    if (findPopup() === 0) return;
    let html = getPopupHtml(element);
    const centerDiv = document.createElement('div');
    centerDiv.id = 'sm-pop-up-model';
    centerDiv.innerHTML = html;
    if (!document.getElementById('sm-pop-up-model')) {
        document.body.appendChild(centerDiv);
    }
    if(snapOptions.changePopup && snapOptions.changePopup !== 'undefined'){
        eval(snapOptions.changePopup);
    }
    if(snapOptions.popupStyle && snapOptions.popupStyle !== 'undefined'){
        addingCssStyle(snapOptions.popupStyle, '#sm-pop-up-model');
    }
    replaceAllValuesInsideClass('downPayment', downPayment);
    replaceAllValuesInsideClass('emiAmount', emi);
    replaceValuesForNonExpress(downPayment, emi);
    const closeBtn = document.getElementById('snapModalCloseon_page');
    if (closeBtn) {
        if (snapOptions.isCartDrawerElementPresent && snapOptions.isCartDrawerElementPresent !== 'undefined') {
            eval(snapOptions.closeCartDrawer);
        }
        closeBtn.addEventListener('click', () => closePopup());
    }
    addPlanType();
    if (snapOptions.isVarientPopup && snapOptions.isVarientPopup === 'true') {
    getVationOnPopup();
     }
    if (snapOptions.isExpress && snapOptions.isExpress === 'true') {
        addingInputFieldKeyup();
    }
    if (
        document.querySelector('.snap_form_first_part') ||
        document.querySelector('.first-inp') ||
        document.querySelector('.snap_form_second_part')
    ) {
        createCart();
        checkOutOfStock();
        if (snapOptions.allowIframeStore && snapOptions.allowIframeStore === 'true') {
            if (!getDynamicJsCookie('snapmintDynamicJscookie')) {
                checkUserInfoFromIframe();
            }
        }
    }
    if (secondWidgetClick(element)) {
        if (document.querySelector('#sm-pop-up-model')) {
            let element = document.querySelector('#sm-pop-up-model');
            element.classList.add('second_widget_popup');
        }
    } else {
        if (snapOptions.isTwoWidget && snapOptions.isTwoWidget === 'true') {
            if (document.querySelector('#sm-pop-up-model')) {
                let element = document.querySelector('#sm-pop-up-model');
                element.classList.add('first_widget_popup');
            }
        }
    }
    if(document.querySelector('#snap-modalon_page'))
    addingCssStyle(snapOptions.style, '#snap-modalon_page')
    if (
        (snapOptions.pdpPopUpCashbackText && snapOptions.pdpPopUpCashbackText !== 'undefined') ||
        (snapOptions.cartPopUpCashbackText && snapOptions.cartPopUpCashbackText !== 'undefined') ||
        (snapOptions.miniCartPopUpCashbackText && snapOptions.miniCartPopUpCashbackText !== 'undefined')
    ) {
        let min_cart = getByIdOrClassName(snapOptions.miniCartDivId);
        if (min_cart) {
            updateTextOnPopUp(snapOptions.miniCartPopUpCashbackText);
        } else if (window.location.href.includes(snapOptions.cartUrl)) {
            updateTextOnPopUp(snapOptions.cartPopUpCashbackText);
        } else {
            updateTextOnPopUp(snapOptions.pdpPopUpCashbackText);
        }
        let checkClickonTerms = event.target.classList;
        if (checkClickonTerms && checkClickonTerms.contains('trm-cond')) {
            clickOnTermsAndCondition();
        }
    }
    if (snapOptions.merchantLogo && snapOptions.merchantLogo !== 'undefined') {
        whiteLabelChange();
    }

    if (snapOptions.isCashbackCoupon && snapOptions.isCashbackCoupon === 'true') {
        let popup = document.querySelector('.coupoun_discount');
        if (popup) {
            orderValueNotChange = snapOrderValue;
            if (snapProducts.length) {
                changeMiniCartOrderValue();
                addingCashbackCoupon();
            } else {
                let count = 0;
                let id = setInterval(() => {
                    count++;
                    if (count === 8 || snapProducts.length > 0) {
                        clearInterval(id);
                        changeMiniCartOrderValue();
                        addingCashbackCoupon();
                    }
                }, 300);
            }
        }
    }
    if (snapOptions.isPosthogEventRequired && snapOptions.isPosthogEventRequired === 'true') {
       let min_cart = getByIdOrClassName(snapOptions.miniCartDivId);
       let id = setTimeout(() => {
           if (snapMintEvent === undefined) {
               var pageSource = "pdp";
           } else if (snapMintEvent != undefined && !window.location.href.includes(snapOptions.cartUrl)) {
               var pageSource = "minicart";
           } else if (window.location.href.includes(snapOptions.cartUrl)) {
               var pageSource = "cart";
           }

           captureEvents('pdp_widget_clicked', {
               merchant_id: snapOptions.mid,
               merchant_name: snapOptions.merchantName,
               source: pageSource,
           });

           if (document.querySelector(snapOptions.checkDisableCheckout) && snapOptions.isExpress === 'true' && snapMintEvent === undefined) {
               var pop_up_type = "size_selection_pending";
           }

           if (!document.querySelector(snapOptions.checkDisableCheckout) && snapOptions.isExpress === 'true' || snapMintEvent != undefined) {
               var pop_up_type = "name_mobile";
           }

           if (snapOptions.isExpress === 'false') {
               var pop_up_type = "normal(1.0)";
           }
           captureEvents('popup_pv', {
               merchant_id: snapOptions.mid,
               merchant_name: snapOptions.merchantName,
               pop_up_type: pop_up_type,
           });
       }, 600);
    }
    if (snapOptions.isGTMEnable && snapOptions.isGTMEnable !== 'undefined') {
        let min_cart = getByIdOrClassName(snapOptions.miniCartDivId);
            var saPAttributes = getCookieAttributes('_shopify_sa_p');
            var saTAttributes = getCookieAttributes('_shopify_sa_t');
            var c_snap_landing_page = getDynamicJsCookie('snap_landing_page');
            var c_snap_referrer_url = getDynamicJsCookie('snap_referrer_url');
            const newSnapProducts = snapProducts?.map(product => ({
                "item_name": product.name,
                "quantity": product.quantity,
                "price": product.unit_price,
                "item_id": product.imei1,
            }));
            dataLayer.push({
                'event': 'snapmint_widget_clicked', 
                values : {
                    'merchant_id': snapOptions.mid, 
                }, 
                'ecommerce': {
                    'items': newSnapProducts
                },
                'page_location': window.location.href,
                'page_path': window.location.pathname,
                'merchant_id': snapOptions.mid,
                items: newSnapProducts,
                'merchant_name': snapOptions.merchantName,
                'order_value': productdesc.snapOrderValue.toString(),
                'orig_referrer': c_snap_referrer_url,
                'landing_page': c_snap_landing_page,
                'shopify_sa_p': saPAttributes.value,
                'shopify_sa_t': saTAttributes.value
            });
    }
    if (snapOptions.addingDeliveryAmountExpress && snapOptions.addingDeliveryAmountExpress !== 'undefined') {
        addingDeliveryAmount();
    }
    if (snapOptions.stopScroll && snapOptions.stopScroll === 'true') {
        document.body.style.overflow = 'hidden';
        document.body.style.height = '';
        document.documentElement.style.overflow = 'hidden';
        if (snapOptions.stopOverflow && snapOptions.stopOverflow !== 'undefined') {
            document.documentElement.style.overflow = 'initial';
        }
    }
    if (snapOptions.modalInputIsNotClickable && snapOptions.modalInputIsNotClickable !== 'undefined') {
        let element = document.querySelector(snapOptions.modalInputIsNotClickable);
        if (element) {
            element.style.visibility = 'hidden';
        }
    }
}

function addingDeliveryAmount() {
    //adding delivery charge for express
    let element = document.querySelector('.delivery-charge-express');
    if (element) {
        document.querySelector('.snap_discounted_hide').style.display = 'block';
        element.innerHTML = snapOrderValue;
        let text = document.querySelector('.delivery-charge-text-express');
        //showing delivery charge text only if delivery charge is less than addingDeliveryMaxAmountExpress
        if (text && parseInt(snapOrderValue) <= parseInt(snapOptions.addingDeliveryMaxAmountExpress)) {
            text.innerHTML = snapOptions.addingDeliveryAmountExpressText;
        }
    }
}
function returnPlanPopup() {
    let element = getByIdOrClassName('#snap-modalon_sale');
    if (element) {
        element.style.display = 'none';
        let element1 = getByIdOrClassName('#snap-modalon_sale_not');
        if (element1) {
            element1.style.display = 'block';
        }
    }
}

function updateTextOnPopUp(text) {
    if (!text) return;
    let showCashBack = document.querySelector('.snap_cashback_section');
    if (showCashBack) {
        showCashBack.style.display = 'flex';
    }
    let element = document.querySelector('.snap_popup_cashback_text');
    if (element) {
        element.innerHTML = text;
    }
}

function clickOnTermsAndCondition() {
    let element = getByIdOrClassName('#snap-modalon_sale');
    if (element) {
        element.style.display = 'flex';
        let text = document.querySelector('.list_items_dynamic');
        if (text && snapOptions.termsAndConditionText && snapOptions.termsAndConditionText !== 'undefined') {
            text.innerHTML = snapOptions.termsAndConditionText;
        }
        let element1 = getByIdOrClassName('#snap-modalon_sale_not');
        if (element1) {
            element1.style.display = 'none';
        }
    }
}
function addPlanType() {
    let allAmount = document.querySelectorAll('.snap_emi_amt');
    if (allAmount.length > 0) {
        if(snapOptions.multiPlanPopupCheck && snapOptions.multiPlanPopupCheck!=='undefined')
        {
            let popUpBoxIndex = 1;
            for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];

            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
                 document.querySelector('.snap_emi_amt_' + popUpBoxIndex).innerText = 'â‚¹' + emi; 
               if (allAmount.length <= 3)
                {   document.querySelector('.snap_emi_month_' + popUpBoxIndex).innerText = 'x ' + plan.tenure + ' months';}
               else
                 {  document.querySelector('.snap_emi_month_' + popUpBoxIndex).innerText = 'x ' + plan.tenure + ' mon'; }

              if (popUpBoxIndex > allAmount.length) {
                    popUpBoxIndex = 1;
                } else {
                    popUpBoxIndex++;
                }
                
            }
            }
        }else
        {
            try{
            for (let planIndex = 0; planIndex < snapOptions.plans.length; planIndex++) {
            const plan = snapOptions.plans[planIndex];
            if (snapOrderValue >= plan.minAmount && snapOrderValue <= plan.maxAmount) {
                var incPlanIndex=planIndex+1;
                emi = Math.round(parseFloat(snapOrderValue * plan.emiPercent) / 100);
               document.querySelector('.snap_emi_amt_' + incPlanIndex).innerText = 'â‚¹' + emi;
               if (allAmount.length <= 3)
                   document.querySelector('.snap_emi_month_' + incPlanIndex).innerText = 'x ' + plan.tenure + ' months';
               else
                   document.querySelector('.snap_emi_month_' + incPlanIndex).innerText = 'x ' + plan.tenure + ' mon';
            }
            }
            }catch(e){}
        }

    }


    let allDate = document.querySelectorAll('.enter_emi_date');
    if (allDate.length > 0) {
        for (let i = 1; i <= allDate.length; i++) {
            var curr_date = new Date().getDate();
            if (curr_date >= 1 && curr_date <= 23) {
                document.querySelector('.snap_emi_date_' + i).innerText = addMonths(new Date(), i);
            } else {
                if (curr_date >= 24 && curr_date <= 31) {
                    document.querySelector('.snap_emi_date_' + i).innerText = addMonths(new Date(), i + 1);
                }
            }
        }
    }

    let total_order_value = document.querySelector('.total_order_value_popup');
    if (total_order_value) {
        total_order_value.innerText = snapOrderValue;
    }
    if (snapOptions.pdpPayLaterAmount && snapOptions.pdpPayLaterAmount !== 'undefined') {
        let remaning_order_value = document.querySelector('.remaning-order-value');
        if (remaning_order_value) {
            remaning_order_value.innerText = 'â‚¹' + parseInt(parseInt(snapOrderValue) - 100);
        }
    }
}
function addChangeEventListener(quantityId, variantId, isMiniCart) {
    let functionCall = isMiniCart ? addminCartLoadPage : loadOnPage;
    if (snapOptions.secondQuantityParentDivId && snapOptions.secondQuantityParentDivId !== 'undefined') {
        if (document.querySelector(snapOptions.secondQuantityParentDivId)) {
            let element = document.querySelector(snapOptions.secondQuantityParentDivId);
            element.addEventListener('click', () => {
                let id = setTimeout(() => {
                    getQuantity(false, snapOptions.secondQuantityDivId);
                    functionCall();
                    clearTimeout(id);
                }, 600);
            });
        }
    }
    if (quantityId && quantityId !== 'undefined') {
        const allElement = document.querySelectorAll(quantityId);
        if (allElement.length > 0) {
            allElement.forEach((element) => {
                if (
                    (window.location.href.includes(snapOptions.cartUrl) || isMiniCart) &&
                    snapOptions.isAJax === 'true'
                ) {
                    let count_timer = 1800;
                    if (snapOptions.timer && snapOptions.timer !== 'undefined') {
                        count_timer = snapOptions.timer;
                    }
                    let eventType = 'click';
                    if (snapOptions.eventType && snapOptions.eventType !== 'undefined') {
                        eventType = snapOptions.eventType;
                    }
                    document.addEventListener(eventType, function (e) {
                        if (
                            window.location.href.includes(snapOptions.cartUrl) ||
                            document.querySelector(snapOptions.miniCartDivId)
                        ) {
                            if (quantityId === 'undefined' || document.querySelectorAll(quantityId).length === 0) {
                                document.removeEventListener('click', function (e) {});
                            }
                            let filter_class = true;
                            let same_class = 0;
                            for (let i = 0; i < e.target.classList.length; i++) {
                                try {
                                    if (!element.querySelector('.' + e.target.classList[i])) {
                                        filter_class = false;
                                        same_class++;
                                    }
                                } catch (e) {}
                            }
                            if (snapOptions.findQuantity && snapOptions.findQuantity !== 'undefined') {
                                if (eval(snapOptions.findQuantity)) {
                                    filter_class = true;
                                }
                            }
                            if (filter_class) {
                                let id = setTimeout(() => {
                                    if(isMiniCart && snapOptions.isSubtotalVisible && snapOptions.isSubtotalVisible !== 'undefined'){
                                        createCartFromMiniCart(true);
                                    }else
                                    functionCall();
                                    if (isMiniCart) {
                                        miniCartQuickViewFn();
                                        if (
                                            snapOptions.isMinCartRemoveReload &&
                                            snapOptions.isMinCartRemoveReload !== 'undefined'
                                        ) {
                                            addListenerForRemoveMiniCartFn();
                                        }
                                    }
                                    clearTimeout(id);
                                }, count_timer);
                            }
                        } else {
                            document.removeEventListener(eventType, function (e) {});
                        }
                    });
                } else {
                    element.addEventListener('click', () => {
                        let id = setTimeout(() => {
                            if (!window.location.href.includes(snapOptions.cartUrl) && !isMiniCart) {
                                getQuantity(isMiniCart);
                            }
                            //removing minicart widget on pdp quantity change
                            let widget = document.querySelector('.snap_emi_txt_minicart');
                            if (widget) {
                                widget.parentNode.removeChild(widget);
                            }
                            functionCall();
                            clearTimeout(id);
                        }, 600);
                    });
                }
            });
        }
    }
    if (variantId) {
        const allElement = document.querySelector(variantId);
        if (allElement) {
            let count_timer = 800;
            if (snapOptions.timer && snapOptions.timer !== 'undefined') {
                count_timer = snapOptions.timer;
            }
            allElement.addEventListener('click', () => {
                let id = setTimeout(() => {
                    if (
                        !window.location.href.includes(snapOptions.cartUrl) &&
                        !isMiniCart &&
                        snapOptions.isQuantity &&
                        snapOptions.isQuantity !== 'undefined'
                    ) {
                        getQuantity(isMiniCart);
                    }
                    if (snapOptions.isWebsiteReload && snapOptions.isWebsiteReload !== 'undefined') {
                        if (!window.location.href.includes(snapOptions.cartUrl) && !isMiniCart) {
                            addChangeEventListener('', snapOptions.variantParentDivId, false);
                        }
                    }
                    functionCall();
                    clearTimeout(id);
                }, count_timer);
            });
        }
    }
}

let count_timer = 0;
let main_id = setInterval(() => {
    if (snapOptions.isAbTest && snapOptions.isAbTest !== 'undefined') {
        if (eval(snapOptions.isAbTest) === false) {
            return;
        }
    }
    if (snapOptions.urlToShowWidget && snapOptions.urlToShowWidget !== 'undefined') {
        if (checkUrlToShowWidget(snapOptions.urlToShowWidget) === false) {
            return;
        }
    }
    count_timer++;
    let divId = false;
    if (getByIdOrClassName(snapOptions.divId)) {
        divId = true;
    }
    if (count_timer === 1) {
        setUtmReferCookie();
    }
    if (count_timer === 10 || divId) {
        clearInterval(main_id);
    }
    if (snapOptions.multiCheckoutBtnId && snapOptions.multiCheckoutBtnId !== 'undefined') {
        if (getByIdOrClassName(snapOptions.multiCheckoutBtnId)) {
            MiniCartOpen(snapOptions.multiCheckoutBtnId, true);
            if (snapOptions.isMultiCheckAjax && snapOptions.isMultiCheckAjax !== 'undefined') {
                let timer = 1500;
                if (snapOptions.multiCheckAjaxTime && snapOptions.multiCheckAjaxTime !== 'undefined') {
                    timer = snapOptions.multiCheckAjaxTime;
                }
                snapOptions.isMultiCheckAjax = snapOptions.multiCheckoutBtnId;
                let ajaxId = setTimeout(() => {
                    MiniCartOpen(snapOptions.isMultiCheckAjax, true);
                    clearTimeout(ajaxId);
                }, timer);
            }
            snapOptions.multiCheckoutBtnId = '';
        }
    }
    if (snapOptions.secondCheckoutBtnId && snapOptions.secondCheckoutBtnId !== 'undefined') {
        if (getByIdOrClassName(snapOptions.secondCheckoutBtnId)) {
            MiniCartOpen(snapOptions.secondCheckoutBtnId);
            snapOptions.secondCheckoutBtnId = '';
        }
    }
    if (snapOptions.thirdCheckoutBtnId && snapOptions.thirdCheckoutBtnId !== 'undefined') {
        if (getByIdOrClassName(snapOptions.thirdCheckoutBtnId)) {
            MiniCartOpen(snapOptions.thirdCheckoutBtnId);
            snapOptions.thirdCheckoutBtnId = '';
        }
    }
    if (snapOptions.multiPdpBtnId && snapOptions.multiPdpBtnId !== 'undefined') {
        if (getByIdOrClassName(snapOptions.multiPdpBtnId)) {
            addingMultiplePdpPage();
            snapOptions.multiPdpBtnId = '';
        }
    }
    //only for pdp quick view button
    if (
        !window.location.href.includes(snapOptions.cartUrl) &&
        snapOptions.isMultiplePdpBtn &&
        snapOptions.isMultiplePdpBtn !== 'undefined' &&
        getByIdOrClassName(snapOptions.isMultiplePdpBtn)
    ) {
        let isMiniCart = getByIdOrClassName(snapOptions.miniCartDivId);
        if (!isMiniCart) {
            addMultiplePdpBtn();
            snapOptions.isMultiplePdpBtn = '';
        }
    }
    if (snapOptions.categoryWidgetInfo && snapOptions.categoryWidgetInfo !== 'undefined') {
        if (getByIdOrClassName(snapOptions.categoryWidgetInfo)) {
            if(snapOptions.categoryWidgetDelay && snapOptions.categoryWidgetDelay !== 'undefined'){
                let id = setTimeout(() => {
                    collectionList(snapOptions.categoryWidgetInfo);
                    snapOptions.categoryWidgetInfo = '';
                    clearTimeout(id);
                }, snapOptions.categoryWidgetDelay);
            }else{
            collectionList(snapOptions.categoryWidgetInfo);
            snapOptions.categoryWidgetInfo = '';
            }
        }
    }
    if(snapOptions.isGTMEnable && snapOptions.isGTMEnable !== 'undefined') {
        loadGtag();
        gtmInitiate();
    }
    if (divId) {
        addChangeEventListener(snapOptions.quantityParentDivId, snapOptions.variantParentDivId, false);
        if (
            snapOptions.isWidgetDelay &&
            snapOptions.isWidgetDelay !== 'undefined' 
         ) {
            let id = setTimeout(() => {
                loadOnPage();
                clearTimeout(id);
            }, snapOptions.isWidgetDelay);
        } else {
            loadOnPage();
        }
        MiniCartOpen();
        clearInterval(main_id);
    }
}, 1000);

function addMultiplePdpBtn() {
    let allBtn = document.querySelectorAll(snapOptions.isMultiplePdpBtn);
    if (allBtn) {
        allBtn.forEach((element) => {
            element.addEventListener('click', () => {
                let all_widget = document.querySelectorAll('#sm-widget-btn');
                if (all_widget) {
                    all_widget.forEach((widget) => {
                        widget.parentNode.removeChild(widget);
                    });
                }
                let id = setTimeout(() => {
                    snapQuantity = 1;
                    addChangeEventListener(snapOptions.quantityParentDivId, snapOptions.variantParentDivId, false);
                    loadOnPage();
                    clearTimeout(id);
                }, 3000);
            });
        });
    }
}

function checkUrlToShowWidget(allUrl) {
    let urlArray = allUrl.split(',');
    let isShowWidget = false;
    if (urlArray && urlArray.length) {
        urlArray.forEach((element) => {
            if (window.location.href.includes(element)) {
                isShowWidget = true;
            }
        });
    }
    return isShowWidget;
}

function addDeliveryAmouunt(snapQuantity) {
    let amount = 0;
    if (snapQuantity) {
        if (snapOptions.deliveryAmount && snapOptions.deliveryAmount !== 'undefined') {
            try {
                if (parseInt(snapOrderValue) < parseInt(snapOptions.minForDelivery)) {
                    amount = parseInt(snapOptions.deliveryAmount) / snapQuantity;
                }
            } catch (e) {}
        }
    }
    return amount;
}

function addingMultiplePdpPage() {
    let pdp_element = document.querySelectorAll(snapOptions.multiPdpBtnId);
    if (pdp_element.length > 0) {
        pdp_element.forEach((element) => {
            element.addEventListener('click', () => {
                let all_widget = document.querySelectorAll('#sm-widget-btn');
                if (all_widget.length > 0) {
                    all_widget.forEach((widget) => {
                        widget.parentNode.removeChild(widget);
                    });
                }
                let id = setTimeout(() => {
                    snapQuantity = 1;
                    snapOptions.divId = snapOptions.secondPdpDivId;
                    addChangeEventListener(snapOptions.quantityParentDivId, snapOptions.variantParentDivId, false);
                    loadOnPage();
                    clearTimeout(id);
                }, 3000);
            });
        });
    }
}
var cartReferenceNode = '';
var windowPopup;
var height = 590;
var width = 790;
var left = (window.screen.width - width) / 2;
var topleft = (window.screen.height - height) / 2;
var windowFeaturesSnapmint = 'width=' + width + ', height=' + height + ', top=' + topleft + ', left=' + left;
var mid = '';
var snapProducts = [];
var productdesc = {};
function getDynamicJsCookie(name) {
    var nameEQ = name + '=';
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
    }
    return null;
}
function setDynamicJsCookie(cname, cvalue) {
    var now = new Date();
    var minutes = 30;
    now.setTime(now.getTime() + minutes * 60 * 1000);
    expires = '; expires=' + now.toUTCString();
    document.cookie = cname + '=' + (cvalue || '') + expires + '; path=/';
}
function getCookieAttributes(cookieName) {
    var cookieAttributes = {
        domain: null,
        path: null,
        value: null
    };
    var cookies = document.cookie.split(';');
    for (var i = 0; i < cookies.length; i++) {
        var cookie = cookies[i].trim();
        if (cookie.indexOf(cookieName + '=') === 0) {
            var cookieParts = cookie.split('=');
            if (cookieParts.length === 2) {
                var cookieValue = cookieParts[1];
                var attributes = cookieValue.split(';');
                for (var j = 0; j < attributes.length; j++) {
                    var attribute = attributes[j].trim();
                    if (attribute.indexOf('Domain=') === 0) {
                        cookieAttributes.domain = attribute.substring('Domain='.length);
                    } else if (attribute.indexOf('Path=') === 0) {
                        cookieAttributes.path = attribute.substring('Path='.length);
                    } else {
                        cookieAttributes.value = decodeURIComponent(cookieValue); // Decode the cookie value
                    }
                }
                break;
            }
        }
    }
    return cookieAttributes;
}
function deliveryFeeSection() {
    let element = document.querySelectorAll('.snap_deliveryfee_section');
    for (let i = 0; i < element.length; i++) {
        element[i].style.display = 'flex';
    }
    let value = document.querySelectorAll('.snap_deliveryfee_value');
    for (let i = 0; i < value.length; i++) {
        value[i].innerText = 'â‚¹' + snapOptions.deliveryAmount;
    }
